=0A=
LRESULT CALLBACK WndProc(HWND hwnd, UINT umessage, WPARAM wparam, LPARAM =
lparam)=0A=
{=0A=
	switch(umessage)=0A=
	{=0A=
		// Check if the window is being destroyed.=0A=
		case WM_DESTROY:=0A=
		{=0A=
			PostQuitMessage(0);=0A=
			return 0;=0A=
