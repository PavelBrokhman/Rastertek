=0A=
	// Setup the windows class with default settings.=0A=
	wc.style         =3D CS_HREDRAW | CS_VREDRAW | CS_OWNDC;=0A=
	wc.lpfnWndProc   =3D WndProc;=0A=
	wc.cbClsExtra    =3D 0;=0A=
	wc.cbWndExtra    =3D 0;=0A=
	wc.hInstance     =3D m_hinstance;=0A=
	wc.hIcon		 =3D LoadIcon(NULL, IDI_WINLOGO);=0A=
	wc.hIconSm       =3D wc.hIcon;=0A=
	wc.hCursor       =3D LoadCursor(NULL, IDC_ARROW);=0A=
	wc.hbrBackground =3D (HBRUSH)GetStockObject(BLACK_BRUSH);=0A=
	wc.lpszMenuName  =3D NULL;=0A=
	wc.lpszClassName =3D m_applicationName;=0A=
	wc.cbSize        =3D sizeof(WNDCLASSEX);=0A=
	=0A=
	// Register the window class.=0A=
	RegisterClassEx(&amp;wc);=0A=
=0A=
	// Determine the resolution of the clients desktop screen.=0A=
	screenWidth  =3D GetSystemMetrics(SM_CXSCREEN);=0A=
	screenHeight =3D GetSystemMetrics(SM_CYSCREEN);=0A=
=0A=
	// Setup the screen settings depending on whether it is running in full =
screen or in windowed mode.=0A=
	if(FULL_SCREEN)=0A=
	{=0A=
		// If full screen set the screen to maximum size of the users desktop =
and 32bit.=0A=
		memset(&amp;dmScreenSettings, 0, sizeof(dmScreenSettings));=0A=
		dmScreenSettings.dmSize       =3D sizeof(dmScreenSettings);=0A=
		dmScreenSettings.dmPelsWidth  =3D (unsigned long)screenWidth;=0A=
		dmScreenSettings.dmPelsHeight =3D (unsigned long)screenHeight;=0A=
		dmScreenSettings.dmBitsPerPel =3D 32;			=0A=
		dmScreenSettings.dmFields     =3D DM_BITSPERPEL | DM_PELSWIDTH | =
DM_PELSHEIGHT;=0A=
=0A=
		// Change the display settings to full screen.=0A=
		ChangeDisplaySettings(&amp;dmScreenSettings, CDS_FULLSCREEN);=0A=
=0A=
		// Set the position of the window to the top left corner.=0A=
		posX =3D posY =3D 0;=0A=
	}=0A=
	else=0A=
	{=0A=
		// If windowed then set it to 800x600 resolution.=0A=
		screenWidth  =3D 800;=0A=
		screenHeight =3D 600;=0A=
=0A=
		// Place the window in the middle of the screen.=0A=
		posX =3D (GetSystemMetrics(SM_CXSCREEN) - screenWidth)  / 2;=0A=
		posY =3D (GetSystemMetrics(SM_CYSCREEN) - screenHeight) / 2;=0A=
	}=0A=
=0A=
	// Create the window with the screen settings and get the handle to it.=0A=
	m_hwnd =3D CreateWindowEx(WS_EX_APPWINDOW, m_applicationName, =
m_applicationName, =0A=
				WS_CLIPSIBLINGS | WS_CLIPCHILDREN | WS_POPUP,=0A=
				posX, posY, screenWidth, screenHeight, NULL, NULL, m_hinstance, =
NULL);=0A=
=0A=
	// Bring the window up on the screen and set it as main focus.=0A=
	ShowWindow(m_hwnd, SW_SHOW);=0A=
	SetForegroundWindow(m_hwnd);=0A=
	SetFocus(m_hwnd);=0A=
=0A=
	// Hide the mouse cursor.=0A=
	ShowCursor(false);=0A=
=0A=
	return;=0A=
}=0A=
=0A=
=0A=
void SystemClass::ShutdownWindows()=0A=
{=0A=
	// Show the mouse cursor.=0A=
