void SystemClass::InitializeWindows(int&amp; screenWidth, int&amp; =
screenHeight)=0A=
{=0A=
	WNDCLASSEX wc;=0A=
	DEVMODE dmScreenSettings;=0A=
	int posX, posY;=0A=
=0A=
=0A=
	// Get an external pointer to this object.	=0A=
	ApplicationHandle =3D this;=0A=
=0A=
	// Get the instance of this application.=0A=
