		delete [] node-&gt;vertexArray;=0A=
		node-&gt;vertexArray =3D 0;=0A=
	}</FONT><FONT color=3D"#aaffaa" face=3D"arial" size=3D"2">=0A=
=0A=
	// Release the four child nodes.=0A=
	for(i=3D0; i&lt;4; i++)=0A=
	{=0A=
		if(node-&gt;nodes[i])=0A=
		{=0A=
			delete node-&gt;nodes[i];=0A=
			node-&gt;nodes[i] =3D 0;=0A=
		}=0A=
	}=0A=
=0A=
	return;=0A=
}=0A=
=0A=
=0A=
void QuadTreeClass::RenderNode(NodeType* node, FrustumClass* frustum, =
ID3D11DeviceContext* deviceContext, TerrainShaderClass* shader)=0A=
{=0A=
	bool result;=0A=
	int count, i, indexCount;=0A=
	unsigned int stride, offset;=0A=
=0A=
=0A=
	// Check to see if the node can be viewed, height doesn't matter in a =
quad tree.=0A=
	result =3D frustum-&gt;CheckCube(node-&gt;positionX, 0.0f, =
node-&gt;positionZ, (node-&gt;width / 2.0f));=0A=
=0A=
	// If it can't be seen then none of its children can either so don't =
continue down the tree, this is where the speed is gained.=0A=
	if(!result)=0A=
	{=0A=
		return;=0A=
	}=0A=
=0A=
	// If it can be seen then check all four child nodes to see if they can =
also be seen.=0A=
	count =3D 0;=0A=
	for(i=3D0; i&lt;4; i++)=0A=
	{=0A=
		if(node-&gt;nodes[i] !=3D 0)=0A=
		{=0A=
			count++;=0A=
			RenderNode(node-&gt;nodes[i], frustum, deviceContext, shader);=0A=
		}=0A=
	}=0A=
=0A=
	// If there were any children nodes then there is no need to continue =
as parent nodes won't contain any triangles to render.=0A=
	if(count !=3D 0)=0A=
	{=0A=
		return;=0A=
	}=0A=
=0A=
	// Otherwise if this node can be seen and has triangles in it then =
render these triangles.=0A=
=0A=
	// Set vertex buffer stride and offset.=0A=
	stride =3D sizeof(VertexType); =0A=
	offset =3D 0;=0A=
=0A=
	// Set the vertex buffer to active in the input assembler so it can be =
rendered.=0A=
	deviceContext-&gt;IASetVertexBuffers(0, 1, &amp;node-&gt;vertexBuffer, =
&amp;stride, &amp;offset);=0A=
=0A=
	// Set the index buffer to active in the input assembler so it can be =
rendered.=0A=
	deviceContext-&gt;IASetIndexBuffer(node-&gt;indexBuffer, =
DXGI_FORMAT_R32_UINT, 0);=0A=
=0A=
	// Set the type of primitive that should be rendered from this vertex =
buffer, in this case triangles.=0A=
	=
deviceContext-&gt;IASetPrimitiveTopology(D3D11_PRIMITIVE_TOPOLOGY_TRIANGL=
ELIST);=0A=
=0A=
	// Determine the number of indices in this node.=0A=
	indexCount =3D node-&gt;triangleCount * 3;=0A=
=0A=
	// Call the terrain shader to render the polygons in this node.=0A=
	shader-&gt;RenderShader(deviceContext, indexCount);=0A=
=0A=
	// Increase the count of the number of polygons that have been rendered =
during this frame.=0A=
	m_drawCount +=3D node-&gt;triangleCount;=0A=
=0A=
	return;=0A=
}=0A=
</FONT></PRE>
<P align=3D"left"><FONT color=3D"#ffffff" face=3D"Arial" size=3D"2">This =
is the first=20
new function which gets the height at a given X and Z position on the =
terrain.
 It first makes sure the position is actually on the terrain mesh before =

proceeding. After that it calls FindNode which will return the height at =
the=20
given position. </FONT></P>
<PRE><FONT color=3D"#aaaaff" face=3D"arial" size=3D"2">bool =
QuadTreeClass::GetHeightAtPosition(float positionX, float positionZ, =
float&amp; height)=0A=
{=0A=
	float meshMinX, meshMaxX, meshMinZ, meshMaxZ;=0A=
=0A=
=0A=
	meshMinX =3D m_parentNode-&gt;positionX - (m_parentNode-&gt;width / =
2.0f);=0A=
	meshMaxX =3D m_parentNode-&gt;positionX + (m_parentNode-&gt;width / =
2.0f);=0A=
=0A=
	meshMinZ =3D m_parentNode-&gt;positionZ - (m_parentNode-&gt;width / =
2.0f);=0A=
	meshMaxZ =3D m_parentNode-&gt;positionZ + (m_parentNode-&gt;width / =
2.0f);=0A=
=0A=
	// Make sure the coordinates are actually over a polygon.=0A=
	if((positionX &lt; meshMinX) || (positionX &gt; meshMaxX) || (positionZ =
&lt; meshMinZ) || (positionZ &gt; meshMaxZ))=0A=
	{=0A=
		return false;=0A=
	}=0A=
=0A=
	// Find the node which contains the polygon for this position.=0A=
	FindNode(m_parentNode, positionX, positionZ, height);=0A=
	=0A=
	return true;=0A=
}=0A=
</FONT></PRE>
<P align=3D"left"><FONT color=3D"#ffffff" face=3D"Arial" =
size=3D"2">FindNode is the next=20
new function which finds the node that the input position is located =
inside of.
 Once it has the node it then searches all the triangles inside by =
calling the=20
CheckHeightOfTriangle which finds the triangle this position is on top =
of. Once=20
it finds that triangle it returns the height. </FONT></P>
<PRE><FONT color=3D"#aaaaff" face=3D"arial" size=3D"2">void =
QuadTreeClass::FindNode(NodeType* node, float x, float z, float&amp; =
height)=0A=
{=0A=
	float xMin, xMax, zMin, zMax;=0A=
	int count, i, index;=0A=
	float vertex1[3], vertex2[3], vertex3[3];=0A=
	bool foundHeight;=0A=
=0A=
=0A=
	// Calculate the dimensions of this node.=0A=
	xMin =3D node-&gt;positionX - (node-&gt;width / 2.0f);=0A=
	xMax =3D node-&gt;positionX + (node-&gt;width / 2.0f);=0A=
=0A=
	zMin =3D node-&gt;positionZ - (node-&gt;width / 2.0f);=0A=
	zMax =3D node-&gt;positionZ + (node-&gt;width / 2.0f);=0A=
=0A=
	// See if the x and z coordinate are in this node, if not then stop =
traversing this part of the tree.=0A=
	if((x &lt; xMin) || (x &gt; xMax) || (z &lt; zMin) || (z &gt; zMax))=0A=
	{=0A=
		return;=0A=
	}=0A=
=0A=
	// If the coordinates are in this node then check first to see if =
children nodes exist.=0A=
	count =3D 0;=0A=
=0A=
	for(i=3D0; i&lt;4; i++)=0A=
	{=0A=
		if(node-&gt;nodes[i] !=3D 0)=0A=
		{=0A=
			count++;=0A=
			FindNode(node-&gt;nodes[i], x, z, height);=0A=
		}=0A=
	}=0A=
=0A=
	// If there were children nodes then return since the polygon will be =
in one of the children.=0A=
	if(count &gt; 0)=0A=
	{=0A=
		return;=0A=
	}=0A=
=0A=
	// If there were no children then the polygon must be in this node.  =
Check all the polygons in this node to find =0A=
	// the height of which one the polygon we are looking for.=0A=
	for(i=3D0; i&lt;node-&gt;triangleCount; i++)=0A=
	{=0A=
		index =3D i * 3;=0A=
		vertex1[0] =3D node-&gt;vertexArray[index].x;=0A=
		vertex1[1] =3D node-&gt;vertexArray[index].y;=0A=
		vertex1[2] =3D node-&gt;vertexArray[index].z;=0A=
=0A=
		index++;=0A=
		vertex2[0] =3D node-&gt;vertexArray[index].x;=0A=
		vertex2[1] =3D node-&gt;vertexArray[index].y;=0A=
		vertex2[2] =3D node-&gt;vertexArray[index].z;=0A=
=0A=
		index++;=0A=
		vertex3[0] =3D node-&gt;vertexArray[index].x;=0A=
		vertex3[1] =3D node-&gt;vertexArray[index].y;=0A=
		vertex3[2] =3D node-&gt;vertexArray[index].z;=0A=
=0A=
		// Check to see if this is the polygon we are looking for.=0A=
		foundHeight =3D CheckHeightOfTriangle(x, z, height, vertex1, vertex2, =
vertex3);=0A=
=0A=
		// If this was the triangle then quit the function and the height will =
be returned to the calling function.=0A=
		if(foundHeight)=0A=
		{=0A=
			return;=0A=
		}=0A=
	}=0A=
=0A=
	return;=0A=
}=0A=
</FONT></PRE>
<P align=3D"left"><FONT color=3D"#ffffff" face=3D"Arial" size=3D"2">The=20
CheckHeightOfTriangle is the final new function in this class which is=20
responsible for determining if a line intersects the given input =
triangle. It=20
takes the three points of the triangle and constructs three lines from =
those=20
three points. Then it tests to see if the position is on the inside of =
all three=20
lines. If it is inside all three lines then an intersection is found and =
the=20
height is calculated and returned. If the point is outside any of the =
three=20
lines then false is returned as there is no intersection of the line =
with the=20
triangle. </FONT></P>
<PRE><FONT color=3D"#aaaaff" face=3D"arial" size=3D"2">bool =
QuadTreeClass::CheckHeightOfTriangle(float x, float z, float&amp; =
height, float v0[3], float v1[3], float v2[3])=0A=
{=0A=
	float startVector[3], directionVector[3], edge1[3], edge2[3], normal[3];=0A=
	float Q[3], e1[3], e2[3], e3[3], edgeNormal[3], temp[3];=0A=
	float magnitude, D, denominator, numerator, t, determinant;=0A=
=0A=
=0A=
	// Starting position of the ray that is being cast.=0A=
	startVector[0] =3D x;=0A=
	startVector[1] =3D 0.0f;=0A=
	startVector[2] =3D z;=0A=
=0A=
	// The direction the ray is being cast.=0A=
	directionVector[0] =3D  0.0f;=0A=
	directionVector[1] =3D -1.0f;=0A=
	directionVector[2] =3D  0.0f;=0A=
=0A=
	// Calculate the two edges from the three points given.=0A=
	edge1[0] =3D v1[0] - v0[0];=0A=
	edge1[1] =3D v1[1] - v0[1];=0A=
	edge1[2] =3D v1[2] - v0[2];=0A=
=0A=
	edge2[0] =3D v2[0] - v0[0];=0A=
	edge2[1] =3D v2[1] - v0[1];=0A=
	edge2[2] =3D v2[2] - v0[2];=0A=
=0A=
	// Calculate the normal of the triangle from the two edges.=0A=
	normal[0] =3D (edge1[1] * edge2[2]) - (edge1[2] * edge2[1]);=0A=
	normal[1] =3D (edge1[2] * edge2[0]) - (edge1[0] * edge2[2]);=0A=
	normal[2] =3D (edge1[0] * edge2[1]) - (edge1[1] * edge2[0]);=0A=
=0A=
	magnitude =3D (float)sqrt((normal[0] * normal[0]) + (normal[1] * =
normal[1]) + (normal[2] * normal[2]));=0A=
	normal[0] =3D normal[0] / magnitude;=0A=
	normal[1] =3D normal[1] / magnitude;=0A=
	normal[2] =3D normal[2] / magnitude;=0A=
=0A=
	// Find the distance from the origin to the plane.=0A=
	D =3D ((-normal[0] * v0[0]) + (-normal[1] * v0[1]) + (-normal[2] * =
v0[2]));=0A=
=0A=
	// Get the denominator of the equation.=0A=
	denominator =3D ((normal[0] * directionVector[0]) + (normal[1] * =
directionVector[1]) + (normal[2] * directionVector[2]));=0A=
=0A=
	// Make sure the result doesn't get too close to zero to prevent divide =
by zero.=0A=
	if(fabs(denominator) &lt; 0.0001f)=0A=
	{=0A=
		return false;=0A=
	}=0A=
=0A=
	// Get the numerator of the equation.=0A=
	numerator =3D -1.0f * (((normal[0] * startVector[0]) + (normal[1] * =
startVector[1]) + (normal[2] * startVector[2])) + D);=0A=
=0A=
	// Calculate where we intersect the triangle.=0A=
	t =3D numerator / denominator;=0A=
=0A=
	// Find the intersection vector.=0A=
	Q[0] =3D startVector[0] + (directionVector[0] * t);=0A=
	Q[1] =3D startVector[1] + (directionVector[1] * t);=0A=
	Q[2] =3D startVector[2] + (directionVector[2] * t);=0A=
=0A=
	// Find the three edges of the triangle.=0A=
	e1[0] =3D v1[0] - v0[0];=0A=
	e1[1] =3D v1[1] - v0[1];=0A=
	e1[2] =3D v1[2] - v0[2];=0A=
=0A=
	e2[0] =3D v2[0] - v1[0];=0A=
	e2[1] =3D v2[1] - v1[1];=0A=
	e2[2] =3D v2[2] - v1[2];=0A=
=0A=
	e3[0] =3D v0[0] - v2[0];=0A=
	e3[1] =3D v0[1] - v2[1];=0A=
	e3[2] =3D v0[2] - v2[2];=0A=
=0A=
	// Calculate the normal for the first edge.=0A=
	edgeNormal[0] =3D (e1[1] * normal[2]) - (e1[2] * normal[1]);=0A=
	edgeNormal[1] =3D (e1[2] * normal[0]) - (e1[0] * normal[2]);=0A=
	edgeNormal[2] =3D (e1[0] * normal[1]) - (e1[1] * normal[0]);=0A=
=0A=
	// Calculate the determinant to see if it is on the inside, outside, or =
directly on the edge.=0A=
	temp[0] =3D Q[0] - v0[0];=0A=
	temp[1] =3D Q[1] - v0[1];=0A=
	temp[2] =3D Q[2] - v0[2];=0A=
=0A=
	determinant =3D ((edgeNormal[0] * temp[0]) + (edgeNormal[1] * temp[1]) =
+ (edgeNormal[2] * temp[2]));=0A=
=0A=
	// Check if it is outside.=0A=
	if(determinant &gt; 0.001f)=0A=
	{=0A=
		return false;=0A=
	}=0A=
=0A=
	// Calculate the normal for the second edge.=0A=
	edgeNormal[0] =3D (e2[1] * normal[2]) - (e2[2] * normal[1]);=0A=
	edgeNormal[1] =3D (e2[2] * normal[0]) - (e2[0] * normal[2]);=0A=
	edgeNormal[2] =3D (e2[0] * normal[1]) - (e2[1] * normal[0]);=0A=
=0A=
	// Calculate the determinant to see if it is on the inside, outside, or =
directly on the edge.=0A=
	temp[0] =3D Q[0] - v1[0];=0A=
	temp[1] =3D Q[1] - v1[1];=0A=
	temp[2] =3D Q[2] - v1[2];=0A=
=0A=
	determinant =3D ((edgeNormal[0] * temp[0]) + (edgeNormal[1] * temp[1]) =
+ (edgeNormal[2] * temp[2]));=0A=
=0A=
	// Check if it is outside.=0A=
	if(determinant &gt; 0.001f)=0A=
	{=0A=
		return false;=0A=
	}=0A=
=0A=
	// Calculate the normal for the third edge.=0A=
	edgeNormal[0] =3D (e3[1] * normal[2]) - (e3[2] * normal[1]);=0A=
	edgeNormal[1] =3D (e3[2] * normal[0]) - (e3[0] * normal[2]);=0A=
	edgeNormal[2] =3D (e3[0] * normal[1]) - (e3[1] * normal[0]);=0A=
=0A=
	// Calculate the determinant to see if it is on the inside, outside, or =
directly on the edge.=0A=
	temp[0] =3D Q[0] - v2[0];=0A=
	temp[1] =3D Q[1] - v2[1];=0A=
	temp[2] =3D Q[2] - v2[2];=0A=
=0A=
	determinant =3D ((edgeNormal[0] * temp[0]) + (edgeNormal[1] * temp[1]) =
+ (edgeNormal[2] * temp[2]));=0A=
=0A=
	// Check if it is outside.=0A=
	if(determinant &gt; 0.001f)=0A=
	{=0A=
		return false;=0A=
	}=0A=
=0A=
	// Now we have our height.=0A=
	height =3D Q[1];=0A=
=0A=
	return true;=0A=
}=0A=
</FONT></PRE><BR>
