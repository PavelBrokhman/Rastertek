}=0A=
=0A=
=0A=
void CameraClass::GenerateBaseViewMatrix()=0A=
{=0A=
	D3DXVECTOR3 up, position, lookAt;=0A=
	float yaw, pitch, roll;=0A=
	D3DXMATRIX rotationMatrix;=0A=
=0A=
=0A=
	// Setup the vector that points upwards.=0A=
	up.x =3D 0.0f;=0A=
	up.y =3D 1.0f;=0A=
	up.z =3D 0.0f;=0A=
=0A=
	// Setup the position of the camera in the world.=0A=
	position.x =3D m_positionX;=0A=
	position.y =3D m_positionY;=0A=
	position.z =3D m_positionZ;=0A=
=0A=
	// Setup where the camera is looking by default.=0A=
	lookAt.x =3D 0.0f;=0A=
	lookAt.y =3D 0.0f;=0A=
	lookAt.z =3D 1.0f;=0A=
=0A=
	// Set the yaw (Y axis), pitch (X axis), and roll (Z axis) rotations in =
radians.=0A=
	pitch =3D m_rotationX * 0.0174532925f;=0A=
	yaw   =3D m_rotationY * 0.0174532925f;=0A=
	roll  =3D m_rotationZ * 0.0174532925f;=0A=
=0A=
	// Create the rotation matrix from the yaw, pitch, and roll values.=0A=
	D3DXMatrixRotationYawPitchRoll(&amp;rotationMatrix, yaw, pitch, roll);=0A=
=0A=
	// Transform the lookAt and up vector by the rotation matrix so the =
view is correctly rotated at the origin.=0A=
	D3DXVec3TransformCoord(&amp;lookAt, &amp;lookAt, &amp;rotationMatrix);=0A=
	D3DXVec3TransformCoord(&amp;up, &amp;up, &amp;rotationMatrix);=0A=
=0A=
	// Translate the rotated camera position to the location of the viewer.=0A=
	lookAt =3D position + lookAt;=0A=
=0A=
	// Finally create the view matrix from the three updated vectors.=0A=
	D3DXMatrixLookAtLH(&amp;m_baseViewMatrix, &amp;position, &amp;lookAt, =
&amp;up);=0A=
=0A=
	return;=0A=
}=0A=
=0A=
=0A=
void CameraClass::GetBaseViewMatrix(D3DXMATRIX&amp; viewMatrix)=0A=
{=0A=
	viewMatrix =3D m_baseViewMatrix;=0A=
	return;=0A=
}=0A=
</FONT></PRE>
<P align=3D"left"><FONT color=3D"#ffffff" face=3D"Arial" size=3D"2">This =
is the new=20
function that is used for generating the reflection view matrix. The =
only=20
difference between the regular view matrix and the reflection one is =
that we=20
invert the Y position based on the height of the plane, and we also =
invert the=20
pitch. </FONT></P>
<PRE><FONT color=3D"#aaaaff" face=3D"arial" size=3D"2">void =
CameraClass::RenderReflection(float height)=0A=
{=0A=
	D3DXVECTOR3 up, position, lookAt;=0A=
	float yaw, pitch, roll;=0A=
	D3DXMATRIX rotationMatrix;=0A=
=0A=
=0A=
	// Setup the vector that points upwards.=0A=
	up.x =3D 0.0f;=0A=
	up.y =3D 1.0f;=0A=
	up.z =3D 0.0f;=0A=
=0A=
	// Setup the position of the camera in the world.  For planar =
reflection invert the Y position of the camera.=0A=
	position.x =3D m_positionX;=0A=
	position.y =3D -m_positionY + (height * 2.0f);=0A=
	position.z =3D m_positionZ;=0A=
=0A=
	// Setup where the camera is looking by default.=0A=
	lookAt.x =3D 0.0f;=0A=
	lookAt.y =3D 0.0f;=0A=
	lookAt.z =3D 1.0f;=0A=
=0A=
	// Set the yaw (Y axis), pitch (X axis), and roll (Z axis) rotations in =
radians.  Invert the X rotation for reflection.=0A=
	pitch =3D -m_rotationX * 0.0174532925f;=0A=
	yaw   =3D m_rotationY * 0.0174532925f;=0A=
	roll  =3D m_rotationZ * 0.0174532925f;=0A=
=0A=
	// Create the rotation matrix from the yaw, pitch, and roll values.=0A=
	D3DXMatrixRotationYawPitchRoll(&amp;rotationMatrix, yaw, pitch, roll);=0A=
=0A=
	// Transform the lookAt and up vector by the rotation matrix so the =
view is correctly rotated at the origin.=0A=
	D3DXVec3TransformCoord(&amp;lookAt, &amp;lookAt, &amp;rotationMatrix);=0A=
	D3DXVec3TransformCoord(&amp;up, &amp;up, &amp;rotationMatrix);=0A=
=0A=
	// Translate the rotated camera position to the location of the viewer.=0A=
	lookAt =3D position + lookAt;=0A=
=0A=
	// Finally create the reflection view matrix from the three updated =
vectors.=0A=
	D3DXMatrixLookAtLH(&amp;m_reflectionViewMatrix, &amp;position, =
&amp;lookAt, &amp;up);=0A=
=0A=
	return;=0A=
}=0A=
</FONT></PRE>
<P align=3D"left"><FONT color=3D"#ffffff" face=3D"Arial" =
size=3D"2">There is also a new=20
function to retrieve the reflection view matrix. </FONT></P>
<PRE><FONT color=3D"#aaaaff" face=3D"arial" size=3D"2">void =
CameraClass::GetReflectionViewMatrix(D3DXMATRIX&amp; viewMatrix)=0A=
{=0A=
	viewMatrix =3D m_reflectionViewMatrix;=0A=
	return;=0A=
}=0A=
</FONT></PRE><BR>
<P align=3D"left"><FONT color=3D"#ffffff" face=3D"Arial"=20
size=3D"3"><U>Reflection.vs</U></FONT></P>
<P align=3D"left"><FONT color=3D"#ffffff" face=3D"Arial" size=3D"2">The =
reflection HLSL=20
shaders are just the terrain HLSL shaders with a clip plane added, =
otherwise=20
they are identical. </FONT></P>
<PRE><FONT color=3D"#aaffaa" face=3D"arial" =
size=3D"2">//////////////////////////////////////////////////////////////=
//////////////////=0A=
// Filename: reflection.vs=0A=
/////////////////////////////////////////////////////////////////////////=
///////=0A=
=0A=
=0A=
/////////////=0A=
// GLOBALS //=0A=
/////////////=0A=
cbuffer MatrixBuffer=0A=
{=0A=
    matrix worldMatrix;=0A=
    matrix viewMatrix;=0A=
    matrix projectionMatrix;=0A=
};</FONT><FONT color=3D"#aaaaff" face=3D"arial" size=3D"2">=0A=
=0A=
cbuffer ClipPlaneBuffer=0A=
{=0A=
    float4 clipPlane;=0A=
};</FONT><FONT color=3D"#aaffaa" face=3D"arial" size=3D"2">=0A=
=0A=
=0A=
//////////////=0A=
// TYPEDEFS //=0A=
//////////////=0A=
struct VertexInputType=0A=
{=0A=
    float4 position : POSITION;=0A=
    float2 tex : TEXCOORD0;=0A=
    float3 normal : NORMAL;=0A=
    float3 tangent : TANGENT;=0A=
    float3 binormal : BINORMAL;=0A=
    float4 color : COLOR;=0A=
};=0A=
=0A=
struct PixelInputType=0A=
{=0A=
    float4 position : SV_POSITION;=0A=
    float2 tex : TEXCOORD0;=0A=
    float3 normal : NORMAL;=0A=
    float3 tangent : TANGENT;=0A=
    float3 binormal : BINORMAL;=0A=
    float4 color : COLOR;</FONT><FONT color=3D"#aaaaff" face=3D"arial" =
size=3D"2">=0A=
    float clip : SV_ClipDistance0;</FONT><FONT color=3D"#aaffaa" =
face=3D"arial" size=3D"2">=0A=
};=0A=
=0A=
=0A=
/////////////////////////////////////////////////////////////////////////=
///////=0A=
// Vertex Shader=0A=
/////////////////////////////////////////////////////////////////////////=
///////=0A=
PixelInputType ReflectionVertexShader(VertexInputType input)=0A=
{=0A=
    PixelInputType output;=0A=
    =0A=
=0A=
    // Change the position vector to be 4 units for proper matrix =
calculations.=0A=
    input.position.w =3D 1.0f;=0A=
=0A=
    // Calculate the position of the vertex against the world, view, and =
projection matrices.=0A=
    output.position =3D mul(input.position, worldMatrix);=0A=
    output.position =3D mul(output.position, viewMatrix);=0A=
    output.position =3D mul(output.position, projectionMatrix);=0A=
    =0A=
    // Store the texture coordinates for the pixel shader.=0A=
    output.tex =3D input.tex;=0A=
    =0A=
    // Calculate the normal vector against the world matrix only and =
then normalize the final value.=0A=
    output.normal =3D mul(input.normal, (float3x3)worldMatrix);=0A=
    output.normal =3D normalize(output.normal);=0A=
=0A=
    // Calculate the tangent vector against the world matrix only and =
then normalize the final value.=0A=
    output.tangent =3D mul(input.tangent, (float3x3)worldMatrix);=0A=
    output.tangent =3D normalize(output.tangent);=0A=
=0A=
    // Calculate the binormal vector against the world matrix only and =
then normalize the final value.=0A=
    output.binormal =3D mul(input.binormal, (float3x3)worldMatrix);=0A=
    output.binormal =3D normalize(output.binormal);=0A=
=0A=
    // Send the color map color into the pixel shader.	=0A=
    output.color =3D input.color;</FONT><FONT color=3D"#aaaaff" =
face=3D"arial" size=3D"2">=0A=
=0A=
    // Set the clipping plane.=0A=
    output.clip =3D dot(mul(input.position, worldMatrix), =
clipPlane);</FONT><FONT color=3D"#aaffaa" face=3D"arial" size=3D"2">=0A=
=0A=
    return output;=0A=
}=0A=
</FONT></PRE><BR>
<P align=3D"left"><FONT color=3D"#ffffff" face=3D"Arial"=20
size=3D"3"><U>Reflection.ps</U></FONT></P>
<PRE><FONT color=3D"#aaffaa" face=3D"arial" =
size=3D"2">//////////////////////////////////////////////////////////////=
//////////////////=0A=
// Filename: reflection.ps=0A=
/////////////////////////////////////////////////////////////////////////=
///////=0A=
=0A=
=0A=
//////////////=0A=
// TEXTURES //=0A=
//////////////=0A=
Texture2D colorTexture : register(t0);=0A=
Texture2D normalTexture : register(t1);=0A=
=0A=
=0A=
//////////////=0A=
// SAMPLERS //=0A=
//////////////=0A=
SamplerState SampleType;=0A=
=0A=
=0A=
//////////////////////=0A=
// CONSTANT BUFFERS //=0A=
//////////////////////=0A=
cbuffer LightBuffer=0A=
{=0A=
    float4 lightDiffuseColor;=0A=
    float3 lightDirection;=0A=
    float colorTextureBrightness;=0A=
};=0A=
=0A=
=0A=
//////////////=0A=
// TYPEDEFS //=0A=
//////////////=0A=
struct PixelInputType=0A=
{=0A=
    float4 position : SV_POSITION;=0A=
    float2 tex : TEXCOORD0;=0A=
    float3 normal : NORMAL;=0A=
    float3 tangent : TANGENT;=0A=
    float3 binormal : BINORMAL;=0A=
    float4 color : COLOR;</FONT><FONT color=3D"#aaaaff" face=3D"arial" =
size=3D"2">=0A=
    float clip : SV_ClipDistance0;</FONT><FONT color=3D"#aaffaa" =
face=3D"arial" size=3D"2">=0A=
};=0A=
=0A=
=0A=
/////////////////////////////////////////////////////////////////////////=
///////=0A=
// Pixel Shader=0A=
/////////////////////////////////////////////////////////////////////////=
///////=0A=
float4 ReflectionPixelShader(PixelInputType input) : SV_TARGET=0A=
{=0A=
    float3 lightDir;=0A=
    float4 textureColor;=0A=
    float4 bumpMap;=0A=
    float3 bumpNormal;=0A=
    float lightIntensity;=0A=
    float4 color;=0A=
=0A=
=0A=
    // Invert the light direction for calculations.=0A=
    lightDir =3D -lightDirection;=0A=
=0A=
    // Sample the color texture.=0A=
    textureColor =3D colorTexture.Sample(SampleType, input.tex);=0A=
=0A=
    // Combine the color map value into the texture color.=0A=
    textureColor =3D saturate(input.color * textureColor * =
colorTextureBrightness);=0A=
=0A=
