=0A=
	// Unlock the constant buffer.=0A=
	deviceContext-&gt;Unmap(m_waterBuffer, 0);=0A=
=0A=
	// Set the position of the water constant buffer in the pixel shader.=0A=
	bufferNumber =3D 0;=0A=
=0A=
	// Finally set the water constant buffer in the pixel shader with the =
