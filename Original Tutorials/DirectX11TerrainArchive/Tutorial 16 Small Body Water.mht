<P align=3D"left"><FONT color=3D"#ffffff" face=3D"Arial"=20
size=3D"3"><U>Cameraclass.cpp</U></FONT></P>
<PRE><FONT color=3D"#aaffaa" face=3D"arial" =
size=3D"2">//////////////////////////////////////////////////////////////=
//////////////////=0A=
// Filename: cameraclass.cpp=0A=
/////////////////////////////////////////////////////////////////////////=
///////=0A=
#include "cameraclass.h"=0A=
=0A=
=0A=
CameraClass::CameraClass()=0A=
{=0A=
	m_positionX =3D 0.0f;=0A=
	m_positionY =3D 0.0f;=0A=
	m_positionZ =3D 0.0f;=0A=
=0A=
	m_rotationX =3D 0.0f;=0A=
	m_rotationY =3D 0.0f;=0A=
	m_rotationZ =3D 0.0f;=0A=
}=0A=
=0A=
=0A=
CameraClass::CameraClass(const CameraClass&amp; other)=0A=
{=0A=
}=0A=
=0A=
=0A=
CameraClass::~CameraClass()=0A=
{=0A=
}=0A=
=0A=
=0A=
void CameraClass::SetPosition(float x, float y, float z)=0A=
{=0A=
	m_positionX =3D x;=0A=
	m_positionY =3D y;=0A=
	m_positionZ =3D z;=0A=
	return;=0A=
}=0A=
=0A=
=0A=
void CameraClass::SetRotation(float x, float y, float z)=0A=
{=0A=
	m_rotationX =3D x;=0A=
	m_rotationY =3D y;=0A=
	m_rotationZ =3D z;=0A=
	return;=0A=
}=0A=
=0A=
=0A=
D3DXVECTOR3 CameraClass::GetPosition()=0A=
{=0A=
	return D3DXVECTOR3(m_positionX, m_positionY, m_positionZ);=0A=
}=0A=
=0A=
=0A=
D3DXVECTOR3 CameraClass::GetRotation()=0A=
{=0A=
	return D3DXVECTOR3(m_rotationX, m_rotationY, m_rotationZ);=0A=
}=0A=
=0A=
=0A=
void CameraClass::Render()=0A=
{=0A=
	D3DXVECTOR3 up, position, lookAt;=0A=
	float yaw, pitch, roll;=0A=
	D3DXMATRIX rotationMatrix;=0A=
=0A=
=0A=
	// Setup the vector that points upwards.=0A=
	up.x =3D 0.0f;=0A=
	up.y =3D 1.0f;=0A=
	up.z =3D 0.0f;=0A=
=0A=
	// Setup the position of the camera in the world.=0A=
	position.x =3D m_positionX;=0A=
	position.y =3D m_positionY;=0A=
	position.z =3D m_positionZ;=0A=
=0A=
	// Setup where the camera is looking by default.=0A=
	lookAt.x =3D 0.0f;=0A=
	lookAt.y =3D 0.0f;=0A=
	lookAt.z =3D 1.0f;=0A=
=0A=
	// Set the yaw (Y axis), pitch (X axis), and roll (Z axis) rotations in =
radians.=0A=
	pitch =3D m_rotationX * 0.0174532925f;=0A=
	yaw   =3D m_rotationY * 0.0174532925f;=0A=
	roll  =3D m_rotationZ * 0.0174532925f;=0A=
=0A=
	// Create the rotation matrix from the yaw, pitch, and roll values.=0A=
	D3DXMatrixRotationYawPitchRoll(&amp;rotationMatrix, yaw, pitch, roll);=0A=
=0A=
	// Transform the lookAt and up vector by the rotation matrix so the =
view is correctly rotated at the origin.=0A=
	D3DXVec3TransformCoord(&amp;lookAt, &amp;lookAt, &amp;rotationMatrix);=0A=
	D3DXVec3TransformCoord(&amp;up, &amp;up, &amp;rotationMatrix);=0A=
=0A=
	// Translate the rotated camera position to the location of the viewer.=0A=
	lookAt =3D position + lookAt;=0A=
=0A=
	// Finally create the view matrix from the three updated vectors.=0A=
	D3DXMatrixLookAtLH(&amp;m_viewMatrix, &amp;position, &amp;lookAt, =
&amp;up);=0A=
=0A=
	return;=0A=
}=0A=
=0A=
=0A=
void CameraClass::GetViewMatrix(D3DXMATRIX&amp; viewMatrix)=0A=
{=0A=
	viewMatrix =3D m_viewMatrix;=0A=
