<PRE><FONT color=3D"#aaffaa" face=3D"arial" size=3D"2">	// Setup the =
description of the gradient constant buffer that is in the pixel shader.=0A=
	gradientBufferDesc.Usage =3D D3D11_USAGE_DYNAMIC;=0A=
	gradientBufferDesc.ByteWidth =3D sizeof(GradientBufferType);=0A=
	gradientBufferDesc.BindFlags =3D D3D11_BIND_CONSTANT_BUFFER;=0A=
	gradientBufferDesc.CPUAccessFlags =3D D3D11_CPU_ACCESS_WRITE;=0A=
	gradientBufferDesc.MiscFlags =3D 0;=0A=
	gradientBufferDesc.StructureByteStride =3D 0;=0A=
=0A=
	// Create the constant buffer pointer so we can access the pixel shader =
constant buffer from within this class.=0A=
	result =3D device-&gt;CreateBuffer(&amp;gradientBufferDesc, NULL, =
&amp;m_gradientBuffer);=0A=
