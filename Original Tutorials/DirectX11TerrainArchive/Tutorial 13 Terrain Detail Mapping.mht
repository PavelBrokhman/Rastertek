			m_heightMap[index].ny =3D (sum[1] / length);=0A=
			m_heightMap[index].nz =3D (sum[2] / length);=0A=
		}=0A=
	}=0A=
=0A=
	// Release the temporary normals.=0A=
	delete [] normals;=0A=
	normals =3D 0;=0A=
=0A=
	return true;=0A=
}=0A=
