=0A=
void TerrainClass::ReduceHeightMap()=0A=
{=0A=
	int i, j;=0A=
=0A=
=0A=
	for(j=3D0; j&lt;m_terrainHeight; j++)=0A=
	{=0A=
		for(i=3D0; i&lt;m_terrainWidth; i++)=0A=
		{=0A=
			m_heightMap[(m_terrainWidth * j) + i].y /=3D 15.0f;=0A=
		}=0A=
