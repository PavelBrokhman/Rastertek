size=3D"3"><U>Terrain.ps</U></FONT></P>
<PRE><FONT color=3D"#aaffaa" face=3D"arial" =
size=3D"2">//////////////////////////////////////////////////////////////=
//////////////////=0A=
// Filename: terrain.ps=0A=
/////////////////////////////////////////////////////////////////////////=
///////=0A=
=0A=
=0A=
//////////////=0A=
// TEXTURES //=0A=
//////////////=0A=
Texture2D colorTexture : register(t0);=0A=
Texture2D normalTexture : register(t1);=0A=
=0A=
=0A=
//////////////=0A=
// SAMPLERS //=0A=
//////////////=0A=
SamplerState SampleType;=0A=
=0A=
=0A=
//////////////////////=0A=
// CONSTANT BUFFERS //=0A=
//////////////////////=0A=
cbuffer LightBuffer=0A=
{=0A=
    float4 diffuseColor;=0A=
    float3 lightDirection;=0A=
    float padding;=0A=
};=0A=
=0A=
=0A=
//////////////=0A=
// TYPEDEFS //=0A=
//////////////=0A=
struct PixelInputType=0A=
{=0A=
    float4 position : SV_POSITION;=0A=
    float2 tex : TEXCOORD0;=0A=
    float3 normal : NORMAL;=0A=
    float3 tangent : TANGENT;=0A=
    float3 binormal : BINORMAL;=0A=
    float4 depthPosition : TEXCOORD1;=0A=
};=0A=
=0A=
=0A=
/////////////////////////////////////////////////////////////////////////=
///////=0A=
// Pixel Shader=0A=
/////////////////////////////////////////////////////////////////////////=
///////=0A=
float4 TerrainPixelShader(PixelInputType input) : SV_TARGET=0A=
{=0A=
    float depthValue;=0A=
    float4 textureColor;=0A=
    float4 bumpMap;=0A=
    float3 bumpNormal;=0A=
    float3 lightDir;=0A=
    float lightIntensity;=0A=
    float4 color;=0A=
=0A=
=0A=
    // Get the depth value of the pixel by dividing the Z pixel depth by =
the homogeneous W coordinate.=0A=
    depthValue =3D input.depthPosition.z / input.depthPosition.w;=0A=
=0A=
    // Sample the texture pixel at this location.=0A=
    textureColor =3D colorTexture.Sample(SampleType, input.tex);=0A=
</FONT></PRE>
<P align=3D"left"><FONT color=3D"#ffffff" face=3D"Arial" size=3D"2">Here =
is the part=20
where we determine whether or not to use bump mapping. We use the depth =
of the=20
pixel in the scene to determine whether to use the normal map texture as =
the=20
normal or to just use the regular lighting normal. </FONT></P>
<PRE><FONT color=3D"#aaffaa" face=3D"arial" size=3D"2">    if(depthValue =
&lt; 0.9f)=0A=
    {    =0A=
        // Sample the pixel in the bump map.=0A=
        bumpMap =3D normalTexture.Sample(SampleType, input.tex);=0A=
=0A=
        // Expand the range of the normal value from (0, +1) to (-1, +1).=0A=
        bumpMap =3D (bumpMap * 2.0f) - 1.0f;=0A=
=0A=
        // Calculate the normal from the data in the bump map.=0A=
        bumpNormal =3D input.normal + bumpMap.x * input.tangent + =
bumpMap.y * input.binormal;=0A=
=0A=
        // Normalize the resulting bump normal.=0A=
        bumpNormal =3D normalize(bumpNormal);=0A=
    }=0A=
    else=0A=
    {=0A=
        bumpNormal =3D input.normal;=0A=
    }	=0A=
	=0A=
    // Invert the light direction for calculations.=0A=
    lightDir =3D -lightDirection;=0A=
=0A=
    // Calculate the amount of light on this pixel based on the bump map =
normal value.=0A=
    lightIntensity =3D saturate(dot(bumpNormal, lightDir));=0A=
=0A=
    // Determine the final diffuse color based on the diffuse color and =
the amount of light intensity.=0A=
    color =3D saturate(diffuseColor * lightIntensity);=0A=
=0A=
    // Combine the final bump light color with the texture color.=0A=
    color =3D color * textureColor;=0A=
=0A=
    return color;=0A=
}=0A=
</FONT></PRE><BR>
