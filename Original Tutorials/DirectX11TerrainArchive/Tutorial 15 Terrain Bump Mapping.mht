<P align=3D"left"><FONT color=3D"#ffffff" face=3D"Arial" size=3D"2">
CalculateTerrainVectors is the function that goes through each triangle =
in the=20
terrain model and calculates and stores the tangent and binormal =
vectors.=20
</FONT></P>
<PRE><FONT color=3D"#aaffaa" face=3D"arial" size=3D"2">void =
TerrainClass::CalculateTerrainVectors()=0A=
{=0A=
	int faceCount, i, index;=0A=
	TempVertexType vertex1, vertex2, vertex3;=0A=
	VectorType tangent, binormal;=0A=
=0A=
=0A=
	// Calculate the number of faces in the terrain model.=0A=
	faceCount =3D m_vertexCount / 3;=0A=
=0A=
	// Initialize the index to the model data.=0A=
	index =3D 0;=0A=
=0A=
	// Go through all the faces and calculate the the tangent, binormal, =
and normal vectors.=0A=
	for(i=3D0; i&lt;faceCount; i++)=0A=
	{=0A=
		// Get the three vertices for this face from the terrain model.=0A=
		vertex1.x  =3D m_TerrainModel[index].x;=0A=
		vertex1.y  =3D m_TerrainModel[index].y;=0A=
		vertex1.z  =3D m_TerrainModel[index].z;=0A=
		vertex1.tu =3D m_TerrainModel[index].tu;=0A=
		vertex1.tv =3D m_TerrainModel[index].tv;=0A=
		vertex1.nx =3D m_TerrainModel[index].nx;=0A=
		vertex1.ny =3D m_TerrainModel[index].ny;=0A=
		vertex1.nz =3D m_TerrainModel[index].nz;=0A=
		index++;=0A=
=0A=
		vertex2.x  =3D m_TerrainModel[index].x;=0A=
		vertex2.y  =3D m_TerrainModel[index].y;=0A=
		vertex2.z  =3D m_TerrainModel[index].z;=0A=
		vertex2.tu =3D m_TerrainModel[index].tu;=0A=
		vertex2.tv =3D m_TerrainModel[index].tv;=0A=
		vertex2.nx =3D m_TerrainModel[index].nx;=0A=
		vertex2.ny =3D m_TerrainModel[index].ny;=0A=
		vertex2.nz =3D m_TerrainModel[index].nz;=0A=
		index++;=0A=
=0A=
		vertex3.x  =3D m_TerrainModel[index].x;=0A=
		vertex3.y  =3D m_TerrainModel[index].y;=0A=
		vertex3.z  =3D m_TerrainModel[index].z;=0A=
		vertex3.tu =3D m_TerrainModel[index].tu;=0A=
		vertex3.tv =3D m_TerrainModel[index].tv;=0A=
		vertex3.nx =3D m_TerrainModel[index].nx;=0A=
		vertex3.ny =3D m_TerrainModel[index].ny;=0A=
		vertex3.nz =3D m_TerrainModel[index].nz;=0A=
		index++;=0A=
=0A=
		// Calculate the tangent and binormal of that face.=0A=
		CalculateTangentBinormal(vertex1, vertex2, vertex3, tangent, binormal);=0A=
=0A=
		// Store the tangent and binormal for this face back in the model =
structure.=0A=
		m_TerrainModel[index-1].tx =3D tangent.x;=0A=
		m_TerrainModel[index-1].ty =3D tangent.y;=0A=
		m_TerrainModel[index-1].tz =3D tangent.z;=0A=
		m_TerrainModel[index-1].bx =3D binormal.x;=0A=
		m_TerrainModel[index-1].by =3D binormal.y;=0A=
		m_TerrainModel[index-1].bz =3D binormal.z;=0A=
=0A=
		m_TerrainModel[index-2].tx =3D tangent.x;=0A=
		m_TerrainModel[index-2].ty =3D tangent.y;=0A=
		m_TerrainModel[index-2].tz =3D tangent.z;=0A=
		m_TerrainModel[index-2].bx =3D binormal.x;=0A=
		m_TerrainModel[index-2].by =3D binormal.y;=0A=
		m_TerrainModel[index-2].bz =3D binormal.z;=0A=
=0A=
		m_TerrainModel[index-3].tx =3D tangent.x;=0A=
		m_TerrainModel[index-3].ty =3D tangent.y;=0A=
		m_TerrainModel[index-3].tz =3D tangent.z;=0A=
		m_TerrainModel[index-3].bx =3D binormal.x;=0A=
		m_TerrainModel[index-3].by =3D binormal.y;=0A=
		m_TerrainModel[index-3].bz =3D binormal.z;=0A=
	}=0A=
=0A=
	return;=0A=
}=0A=
=0A=
=0A=
void TerrainClass::CalculateTangentBinormal(TempVertexType vertex1, =
TempVertexType vertex2, TempVertexType vertex3, VectorType&amp; tangent, =0A=
					    VectorType&amp; binormal)=0A=
{=0A=
	float vector1[3], vector2[3];=0A=
	float tuVector[2], tvVector[2];=0A=
	float den;=0A=
	float length;=0A=
=0A=
=0A=
	// Calculate the two vectors for this face.=0A=
	vector1[0] =3D vertex2.x - vertex1.x;=0A=
	vector1[1] =3D vertex2.y - vertex1.y;=0A=
	vector1[2] =3D vertex2.z - vertex1.z;=0A=
=0A=
	vector2[0] =3D vertex3.x - vertex1.x;=0A=
	vector2[1] =3D vertex3.y - vertex1.y;=0A=
	vector2[2] =3D vertex3.z - vertex1.z;=0A=
=0A=
	// Calculate the tu and tv texture space vectors.=0A=
	tuVector[0] =3D vertex2.tu - vertex1.tu;=0A=
	tvVector[0] =3D vertex2.tv - vertex1.tv;=0A=
=0A=
	tuVector[1] =3D vertex3.tu - vertex1.tu;=0A=
	tvVector[1] =3D vertex3.tv - vertex1.tv;=0A=
=0A=
	// Calculate the denominator of the tangent/binormal equation.=0A=
	den =3D 1.0f / (tuVector[0] * tvVector[1] - tuVector[1] * tvVector[0]);=0A=
=0A=
	// Calculate the cross products and multiply by the coefficient to get =
the tangent and binormal.=0A=
	tangent.x =3D (tvVector[1] * vector1[0] - tvVector[0] * vector2[0]) * =
den;=0A=
	tangent.y =3D (tvVector[1] * vector1[1] - tvVector[0] * vector2[1]) * =
den;=0A=
	tangent.z =3D (tvVector[1] * vector1[2] - tvVector[0] * vector2[2]) * =
den;=0A=
=0A=
	binormal.x =3D (tuVector[0] * vector2[0] - tuVector[1] * vector1[0]) * =
den;=0A=
	binormal.y =3D (tuVector[0] * vector2[1] - tuVector[1] * vector1[1]) * =
den;=0A=
	binormal.z =3D (tuVector[0] * vector2[2] - tuVector[1] * vector1[2]) * =
den;=0A=
=0A=
	// Calculate the length of this normal.=0A=
	length =3D sqrt((tangent.x * tangent.x) + (tangent.y * tangent.y) + =
(tangent.z * tangent.z));=0A=
			=0A=
	// Normalize the normal and then store it=0A=
	tangent.x =3D tangent.x / length;=0A=
	tangent.y =3D tangent.y / length;=0A=
	tangent.z =3D tangent.z / length;=0A=
=0A=
	// Calculate the length of this normal.=0A=
	length =3D sqrt((binormal.x * binormal.x) + (binormal.y * binormal.y) + =
(binormal.z * binormal.z));=0A=
			=0A=
	// Normalize the normal and then store it=0A=
	binormal.x =3D binormal.x / length;=0A=
	binormal.y =3D binormal.y / length;=0A=
	binormal.z =3D binormal.z / length;=0A=
=0A=
	return;=0A=
}=0A=
</FONT></PRE>
<P align=3D"left"><FONT color=3D"#ffffff" face=3D"Arial" size=3D"2">The=20
InitializeBuffers function has been modified to build the vertex and =
index=20
buffer from the terrain model instead of the height map data. =
</FONT></P>
<PRE><FONT color=3D"#aaffaa" face=3D"arial" size=3D"2">bool =
TerrainClass::InitializeBuffers(ID3D11Device* device)=0A=
{=0A=
	VertexType* vertices;=0A=
	unsigned long* indices;=0A=
	int i;=0A=
	D3D11_BUFFER_DESC vertexBufferDesc, indexBufferDesc;=0A=
	D3D11_SUBRESOURCE_DATA vertexData, indexData;=0A=
	HRESULT result;=0A=
=0A=
=0A=
	// Set the index count to the same as the vertex count.=0A=
	m_indexCount =3D m_vertexCount;=0A=
=0A=
	// Create the vertex array.=0A=
	vertices =3D new VertexType[m_vertexCount];=0A=
	if(!vertices)=0A=
	{=0A=
		return false;=0A=
	}=0A=
=0A=
	// Create the index array.=0A=
	indices =3D new unsigned long[m_indexCount];=0A=
	if(!indices)=0A=
	{=0A=
		return false;=0A=
	}=0A=
=0A=
	// Load the vertex and index array with data from the terrain model.=0A=
	for(i=3D0; i&lt;m_vertexCount; i++)=0A=
	{=0A=
		vertices[i].position =3D D3DXVECTOR3(m_TerrainModel[i].x, =
m_TerrainModel[i].y, m_TerrainModel[i].z);=0A=
		vertices[i].texture =3D D3DXVECTOR2(m_TerrainModel[i].tu, =
m_TerrainModel[i].tv);=0A=
		vertices[i].normal =3D D3DXVECTOR3(m_TerrainModel[i].nx, =
m_TerrainModel[i].ny, m_TerrainModel[i].nz);=0A=
		vertices[i].tangent =3D D3DXVECTOR3(m_TerrainModel[i].tx, =
m_TerrainModel[i].ty, m_TerrainModel[i].tz);=0A=
		vertices[i].binormal =3D D3DXVECTOR3(m_TerrainModel[i].bx, =
m_TerrainModel[i].by, m_TerrainModel[i].bz);=0A=
=0A=
		indices[i] =3D i;=0A=
	}=0A=
=0A=
	// Set up the description of the static vertex buffer.=0A=
	vertexBufferDesc.Usage =3D D3D11_USAGE_DEFAULT;=0A=
	vertexBufferDesc.ByteWidth =3D sizeof(VertexType) * m_vertexCount;=0A=
	vertexBufferDesc.BindFlags =3D D3D11_BIND_VERTEX_BUFFER;=0A=
	vertexBufferDesc.CPUAccessFlags =3D 0;=0A=
	vertexBufferDesc.MiscFlags =3D 0;=0A=
	vertexBufferDesc.StructureByteStride =3D 0;=0A=
=0A=
	// Give the subresource structure a pointer to the vertex data.=0A=
	vertexData.pSysMem =3D vertices;=0A=
	vertexData.SysMemPitch =3D 0;=0A=
	vertexData.SysMemSlicePitch =3D 0;=0A=
=0A=
	// Now create the vertex buffer.=0A=
	result =3D device-&gt;CreateBuffer(&amp;vertexBufferDesc, =
&amp;vertexData, &amp;m_vertexBuffer);=0A=
	if(FAILED(result))=0A=
	{=0A=
		return false;=0A=
	}=0A=
=0A=
	// Set up the description of the static index buffer.=0A=
	indexBufferDesc.Usage =3D D3D11_USAGE_DEFAULT;=0A=
	indexBufferDesc.ByteWidth =3D sizeof(unsigned long) * m_indexCount;=0A=
	indexBufferDesc.BindFlags =3D D3D11_BIND_INDEX_BUFFER;=0A=
	indexBufferDesc.CPUAccessFlags =3D 0;=0A=
	indexBufferDesc.MiscFlags =3D 0;=0A=
	indexBufferDesc.StructureByteStride =3D 0;=0A=
=0A=
	// Give the subresource structure a pointer to the index data.=0A=
	indexData.pSysMem =3D indices;=0A=
	indexData.SysMemPitch =3D 0;=0A=
	indexData.SysMemSlicePitch =3D 0;=0A=
=0A=
	// Create the index buffer.=0A=
	result =3D device-&gt;CreateBuffer(&amp;indexBufferDesc, =
&amp;indexData, &amp;m_indexBuffer);=0A=
	if(FAILED(result))=0A=
