{=0A=
}=0A=
=0A=
=0A=
bool MiniMapClass::Initialize(ID3D11Device* device, HWND hwnd, int =
screenWidth, int screenHeight, D3DXMATRIX viewMatrix, float =
terrainWidth, =0A=
			      float terrainHeight)=0A=
{=0A=
	bool result;=0A=
</FONT></PRE>
<P align=3D"left"><FONT color=3D"#ffffff" face=3D"Arial" size=3D"2">The =
Initialize=20
function starts by storing the location of the mini-map, its size, the =
base view=20
matrix for rendering, and the actual size of the terrain mesh. =
</FONT></P>
<PRE><FONT color=3D"#aaffaa" face=3D"arial" size=3D"2">	// Initialize =
the location of the mini-map on the screen.=0A=
	m_mapLocationX =3D 150;=0A=
	m_mapLocationY =3D 75;=0A=
=0A=
	// Set the size of the mini-map.=0A=
	m_mapSizeX =3D 150.0f;=0A=
	m_mapSizeY =3D 150.0f;=0A=
=0A=
	// Store the base view matrix.=0A=
	m_viewMatrix =3D viewMatrix;=0A=
=0A=
	// Store the terrain size.=0A=
	m_terrainWidth =3D terrainWidth;=0A=
	m_terrainHeight =3D terrainHeight;=0A=
</FONT></PRE>
<P align=3D"left"><FONT color=3D"#ffffff" face=3D"Arial" size=3D"2">Next =
we create the=20
mini-map from the color map .dds file. Even thought the color map is =
256x256 we=20
have shrunk it down to 150x150 so that displaying it does not take up =
too much=20
of the screen. </FONT></P>
<PRE><FONT color=3D"#aaffaa" face=3D"arial" size=3D"2">	// Create the =
mini-map bitmap object.=0A=
	m_MiniMapBitmap =3D new BitmapClass;=0A=
	if(!m_MiniMapBitmap)=0A=
	{=0A=
		return false;=0A=
	}=0A=
=0A=
	// Initialize the mini-map bitmap object.=0A=
	result =3D m_MiniMapBitmap-&gt;Initialize(device, screenWidth, =
screenHeight, L"../Engine/data/colorm01.dds", 150, 150);=0A=
	if(!result)=0A=
	{=0A=
		MessageBox(hwnd, L"Could not initialize the mini-map object.", =
L"Error", MB_OK);=0A=
		return false;=0A=
	}=0A=
</FONT></PRE>
<P align=3D"left"><FONT color=3D"#ffffff" face=3D"Arial" =
size=3D"2">After that we load=20
the white border which will surround the mini-map so the edges are =
clearly=20
defined. </FONT></P>
<PRE><FONT color=3D"#aaffaa" face=3D"arial" size=3D"2">	// Create the =
border bitmap object.=0A=
	m_Border =3D new BitmapClass;=0A=
	if(!m_Border)=0A=
	{=0A=
		return false;=0A=
	}=0A=
=0A=
	// Initialize the border bitmap object.=0A=
	result =3D m_Border-&gt;Initialize(device, screenWidth, screenHeight, =
L"../Engine/data/border01.dds", 154, 154);=0A=
	if(!result)=0A=
	{=0A=
		MessageBox(hwnd, L"Could not initialize the border object.", L"Error", =
MB_OK);=0A=
		return false;=0A=
	}=0A=
</FONT></PRE>
<P align=3D"left"><FONT color=3D"#ffffff" face=3D"Arial" size=3D"2">And =
finally we load=20
the point indicator. It is a 3x3 green pixel that we will use and =
constantly=20
update to show where the user currently is located on the mini-map. =
</FONT></P>
<PRE><FONT color=3D"#aaffaa" face=3D"arial" size=3D"2">	// Create the =
point bitmap object.=0A=
	m_Point =3D new BitmapClass;=0A=
	if(!m_Point)=0A=
	{=0A=
		return false;=0A=
	}=0A=
=0A=
	// Initialize the point bitmap object.=0A=
	result =3D m_Point-&gt;Initialize(device, screenWidth, screenHeight, =
L"../Engine/data/point01.dds", 3, 3);=0A=
	if(!result)=0A=
	{=0A=
		MessageBox(hwnd, L"Could not initialize the point object.", L"Error", =
MB_OK);=0A=
		return false;=0A=
	}=0A=
=0A=
	return true;=0A=
}=0A=
</FONT></PRE>
<P align=3D"left"><FONT color=3D"#ffffff" face=3D"Arial" size=3D"2">The =
Shutdown=20
function releases the three bitmap objects that were used for rendering =
the=20
mini-map elements. </FONT></P>
<PRE><FONT color=3D"#aaffaa" face=3D"arial" size=3D"2">void =
MiniMapClass::Shutdown()=0A=
{=0A=
	// Release the point bitmap object.=0A=
	if(m_Point)=0A=
	{=0A=
		m_Point-&gt;Shutdown();=0A=
		delete m_Point;=0A=
		m_Point =3D 0;=0A=
	}=0A=
=0A=
	// Release the border bitmap object.=0A=
	if(m_Border)=0A=
	{=0A=
		m_Border-&gt;Shutdown();=0A=
		delete m_Border;=0A=
		m_Border =3D 0;=0A=
	}=0A=
=0A=
	// Release the mini-map bitmap object.=0A=
	if(m_MiniMapBitmap)=0A=
	{=0A=
		m_MiniMapBitmap-&gt;Shutdown();=0A=
		delete m_MiniMapBitmap;=0A=
		m_MiniMapBitmap =3D 0;=0A=
	}=0A=
=0A=
	return;=0A=
}=0A=
=0A=
=0A=
bool MiniMapClass::Render(ID3D11DeviceContext* deviceContext, D3DXMATRIX =
worldMatrix, D3DXMATRIX orthoMatrix, TextureShaderClass* textureShader)=0A=
{=0A=
	bool result;=0A=
</FONT></PRE>
<P align=3D"left"><FONT color=3D"#ffffff" face=3D"Arial" size=3D"2">The =
first thing we=20
render is the border for the mini-map. </FONT></P>
<PRE><FONT color=3D"#aaffaa" face=3D"arial" size=3D"2">	// Put the =
border bitmap vertex and index buffers on the graphics pipeline to =
prepare them for drawing.=0A=
	result =3D m_Border-&gt;Render(deviceContext, (m_mapLocationX - 2), =
(m_mapLocationY - 2));=0A=
	if(!result)=0A=
	{=0A=
		return false;=0A=
	}=0A=
=0A=
	// Render the border bitmap using the texture shader.=0A=
	textureShader-&gt;Render(deviceContext, m_Border-&gt;GetIndexCount(), =
worldMatrix, m_viewMatrix, orthoMatrix, m_Border-&gt;GetTexture());=0A=
</FONT></PRE>
<P align=3D"left"><FONT color=3D"#ffffff" face=3D"Arial" =
size=3D"2">After that the color=20
map is rendered as the actual mini-map. </FONT></P>
<PRE><FONT color=3D"#aaffaa" face=3D"arial" size=3D"2">	// Put the =
mini-map bitmap vertex and index buffers on the graphics pipeline to =
prepare them for drawing.=0A=
	result =3D m_MiniMapBitmap-&gt;Render(deviceContext, m_mapLocationX, =
m_mapLocationY);=0A=
	if(!result)=0A=
	{=0A=
		return false;=0A=
	}=0A=
=0A=
	// Render the mini-map bitmap using the texture shader.=0A=
	textureShader-&gt;Render(deviceContext, =
m_MiniMapBitmap-&gt;GetIndexCount(), worldMatrix, m_viewMatrix, =
orthoMatrix, m_MiniMapBitmap-&gt;GetTexture());=0A=
</FONT></PRE>
<P align=3D"left"><FONT color=3D"#ffffff" face=3D"Arial" size=3D"2">And =
finally the=20
point indicator is rendered on the mini-map according the where the user =
is=20
currently located on the 3D terrain mesh. </FONT></P>
<PRE><FONT color=3D"#aaffaa" face=3D"arial" size=3D"2">	// Put the point =
bitmap vertex and index buffers on the graphics pipeline to prepare them =
for drawing.=0A=
	result =3D m_Point-&gt;Render(deviceContext, m_pointLocationX, =
m_pointLocationY);=0A=
	if(!result)=0A=
	{=0A=
		return false;=0A=
	}=0A=
=0A=
	// Render the point bitmap using the texture shader.=0A=
	textureShader-&gt;Render(deviceContext, m_Point-&gt;GetIndexCount(), =
worldMatrix, m_viewMatrix, orthoMatrix, m_Point-&gt;GetTexture());=0A=
=0A=
	return true;=0A=
}=0A=
</FONT></PRE>
<P align=3D"left"><FONT color=3D"#ffffff" face=3D"Arial" size=3D"2">The =
PositionUpdate=20
function is used for updating where the 3x3 green pixel point indicator =
should=20
be located on the mini-map. It converts the 3D float position of the =
camera on=20
the terrain into a 2D position on the bitmap. It also makes sure the =
indicator=20
never goes past the borders of the mini-map. </FONT></P>
<PRE><FONT color=3D"#aaffaa" face=3D"arial" size=3D"2">void =
MiniMapClass::PositionUpdate(float positionX, float positionZ)=0A=
{=0A=
	float percentX, percentY;=0A=
=0A=
=0A=
	// Ensure the point does not leave the minimap borders even if the =
camera goes past the terrain borders.=0A=
	if(positionX &lt; 0)=0A=
	{=0A=
		positionX =3D 0;=0A=
	}=0A=
=0A=
	if(positionZ &lt; 0)=0A=
	{=0A=
		positionZ =3D 0;=0A=
	}=0A=
=0A=
	if(positionX &gt; m_terrainWidth)=0A=
	{=0A=
		positionX =3D m_terrainWidth;=0A=
	}=0A=
=0A=
	if(positionZ &gt; m_terrainHeight)=0A=
	{=0A=
		positionZ =3D m_terrainHeight;=0A=
	}=0A=
=0A=
	// Calculate the position of the camera on the minimap in terms of =
percentage.=0A=
	percentX =3D positionX / m_terrainWidth;=0A=
	percentY =3D 1.0f - (positionZ / m_terrainHeight);=0A=
=0A=
	// Determine the pixel location of the point on the mini-map.=0A=
	m_pointLocationX =3D m_mapLocationX + (int)(percentX * m_mapSizeX);=0A=
	m_pointLocationY =3D m_mapLocationY + (int)(percentY * m_mapSizeY);=0A=
=0A=
	// Subtract one from the location to center the point on the mini-map =
according to the 3x3 point pixel image size.=0A=
	m_pointLocationX =3D m_pointLocationX - 1;=0A=
	m_pointLocationY =3D m_pointLocationY - 1;=0A=
=0A=
	return;=0A=
}=0A=
</FONT></PRE><BR>
