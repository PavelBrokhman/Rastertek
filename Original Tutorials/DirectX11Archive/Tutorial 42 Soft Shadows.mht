=0A=
=0A=
/////////////////////////////////////////////////////////////////////////=
///////=0A=
// Vertex Shader=0A=
/////////////////////////////////////////////////////////////////////////=
///////=0A=
PixelInputType ShadowVertexShader(VertexInputType input)=0A=
{=0A=
    PixelInputType output;=0A=
    float4 worldPosition;=0A=
    =0A=
    =0A=
    // Change the position vector to be 4 units for proper matrix =
calculations.=0A=
    input.position.w =3D 1.0f;=0A=
=0A=
    // Calculate the position of the vertex against the world, view, and =
projection matrices.=0A=
    output.position =3D mul(input.position, worldMatrix);=0A=
    output.position =3D mul(output.position, viewMatrix);=0A=
    output.position =3D mul(output.position, projectionMatrix);=0A=
    =0A=
    // Calculate the position of the vertice as viewed by the light =
source.=0A=
    output.lightViewPosition =3D mul(input.position, worldMatrix);=0A=
    output.lightViewPosition =3D mul(output.lightViewPosition, =
lightViewMatrix);=0A=
    output.lightViewPosition =3D mul(output.lightViewPosition, =
lightProjectionMatrix);=0A=
=0A=
    // Store the texture coordinates for the pixel shader.=0A=
    output.tex =3D input.tex;=0A=
    =0A=
    // Calculate the normal vector against the world matrix only.=0A=
    output.normal =3D mul(input.normal, (float3x3)worldMatrix);=0A=
	=0A=
    // Normalize the normal vector.=0A=
    output.normal =3D normalize(output.normal);=0A=
=0A=
    // Calculate the position of the vertex in the world.=0A=
    worldPosition =3D mul(input.position, worldMatrix);=0A=
=0A=
    // Determine the light position based on the position of the light =
and the position of the vertex in the world.=0A=
    output.lightPos =3D lightPosition.xyz - worldPosition.xyz;=0A=
=0A=
    // Normalize the light position vector.=0A=
    output.lightPos =3D normalize(output.lightPos);=0A=
=0A=
    return output;=0A=
}=0A=
