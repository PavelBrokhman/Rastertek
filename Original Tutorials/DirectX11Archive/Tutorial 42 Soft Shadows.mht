=0A=
bool ShadowShaderClass::SetShaderParameters(ID3D11DeviceContext* =
deviceContext, D3DXMATRIX worldMatrix, D3DXMATRIX viewMatrix, =0A=
					    D3DXMATRIX projectionMatrix, D3DXMATRIX lightViewMatrix, =
D3DXMATRIX lightProjectionMatrix, =0A=
					    ID3D11ShaderResourceView* depthMapTexture, D3DXVECTOR3 =
lightPosition)=0A=
{=0A=
	HRESULT result;=0A=
	D3D11_MAPPED_SUBRESOURCE mappedResource;=0A=
	unsigned int bufferNumber;=0A=
	MatrixBufferType* dataPtr;=0A=
	LightBufferType2* dataPtr3;=0A=
=0A=
=0A=
	// Transpose the matrices to prepare them for the shader.=0A=
	D3DXMatrixTranspose(&amp;worldMatrix, &amp;worldMatrix);=0A=
	D3DXMatrixTranspose(&amp;viewMatrix, &amp;viewMatrix);=0A=
	D3DXMatrixTranspose(&amp;projectionMatrix, &amp;projectionMatrix);=0A=
	D3DXMatrixTranspose(&amp;lightViewMatrix, &amp;lightViewMatrix);=0A=
	D3DXMatrixTranspose(&amp;lightProjectionMatrix, =
&amp;lightProjectionMatrix);=0A=
=0A=
	// Lock the constant buffer so it can be written to.=0A=
	result =3D deviceContext-&gt;Map(m_matrixBuffer, 0, =
D3D11_MAP_WRITE_DISCARD, 0, &amp;mappedResource);=0A=
	if(FAILED(result))=0A=
	{=0A=
		return false;=0A=
	}=0A=
=0A=
	// Get a pointer to the data in the constant buffer.=0A=
	dataPtr =3D (MatrixBufferType*)mappedResource.pData;=0A=
=0A=
	// Copy the matrices into the constant buffer.=0A=
	dataPtr-&gt;world =3D worldMatrix;=0A=
	dataPtr-&gt;view =3D viewMatrix;=0A=
	dataPtr-&gt;projection =3D projectionMatrix;=0A=
	dataPtr-&gt;lightView =3D lightViewMatrix;=0A=
	dataPtr-&gt;lightProjection =3D lightProjectionMatrix;=0A=
=0A=
	// Unlock the constant buffer.=0A=
	deviceContext-&gt;Unmap(m_matrixBuffer, 0);=0A=
=0A=
	// Set the position of the constant buffer in the vertex shader.=0A=
	bufferNumber =3D 0;=0A=
=0A=
	// Now set the constant buffer in the vertex shader with the updated =
values.=0A=
	deviceContext-&gt;VSSetConstantBuffers(bufferNumber, 1, =
&amp;m_matrixBuffer);=0A=
=0A=
	// Set shader texture resource in the pixel shader.=0A=
	deviceContext-&gt;PSSetShaderResources(0, 1, &amp;depthMapTexture);=0A=
=0A=
	// Lock the second light constant buffer so it can be written to.=0A=
	result =3D deviceContext-&gt;Map(m_lightBuffer2, 0, =
D3D11_MAP_WRITE_DISCARD, 0, &amp;mappedResource);=0A=
	if(FAILED(result))=0A=
	{=0A=
		return false;=0A=
	}=0A=
=0A=
	// Get a pointer to the data in the constant buffer.=0A=
	dataPtr3 =3D (LightBufferType2*)mappedResource.pData;=0A=
=0A=
	// Copy the lighting variables into the constant buffer.=0A=
	dataPtr3-&gt;lightPosition =3D lightPosition;=0A=
	dataPtr3-&gt;padding =3D 0.0f;=0A=
=0A=
	// Unlock the constant buffer.=0A=
	deviceContext-&gt;Unmap(m_lightBuffer2, 0);=0A=
=0A=
	// Set the position of the light constant buffer in the vertex shader.=0A=
	bufferNumber =3D 1;=0A=
=0A=
	// Finally set the light constant buffer in the pixel shader with the =
updated values.=0A=
	deviceContext-&gt;VSSetConstantBuffers(bufferNumber, 1, =
&amp;m_lightBuffer2);=0A=
=0A=
	return true;=0A=
}=0A=
=0A=
=0A=
void ShadowShaderClass::RenderShader(ID3D11DeviceContext* deviceContext, =
int indexCount)=0A=
{=0A=
	// Set the vertex input layout.=0A=
	deviceContext-&gt;IASetInputLayout(m_layout);=0A=
=0A=
	// Set the vertex and pixel shaders that will be used to render this =
triangle.=0A=
	deviceContext-&gt;VSSetShader(m_vertexShader, NULL, 0);=0A=
	deviceContext-&gt;PSSetShader(m_pixelShader, NULL, 0);=0A=
=0A=
	// Set the sampler states in the pixel shader.=0A=
	deviceContext-&gt;PSSetSamplers(0, 1, &amp;m_sampleStateClamp);=0A=
=0A=
	// Render the triangle.=0A=
	deviceContext-&gt;DrawIndexed(indexCount, 0, 0);=0A=
=0A=
	return;=0A=
}=0A=
</FONT></PRE><BR>
<P align=3D"left"><FONT color=3D"#ffffff" face=3D"Arial"=20
size=3D"3"><U>Softshadow.vs</U></FONT></P>
<P align=3D"left"><FONT color=3D"#ffffff" face=3D"Arial" size=3D"2">The =
soft shadow=20
shaders are where we light the scene as normal but then project the =
blurred=20
black and white shadow  texture onto the scene to complete the soft =
shadow=20
effect. </FONT></P>
<PRE><FONT color=3D"#aaffaa" face=3D"arial" =
size=3D"2">//////////////////////////////////////////////////////////////=
//////////////////=0A=
// Filename: softshadow.vs=0A=
/////////////////////////////////////////////////////////////////////////=
///////=0A=
=0A=
=0A=
/////////////=0A=
// GLOBALS //=0A=
/////////////=0A=
cbuffer MatrixBuffer=0A=
{=0A=
    matrix worldMatrix;=0A=
    matrix viewMatrix;=0A=
    matrix projectionMatrix;=0A=
};=0A=
=0A=
=0A=
//////////////////////=0A=
// CONSTANT BUFFERS //=0A=
//////////////////////=0A=
cbuffer LightBuffer2=0A=
{=0A=
    float3 lightPosition;=0A=
    float padding;=0A=
};=0A=
=0A=
=0A=
//////////////=0A=
// TYPEDEFS //=0A=
//////////////=0A=
struct VertexInputType=0A=
{=0A=
    float4 position : POSITION;=0A=
    float2 tex : TEXCOORD0;=0A=
    float3 normal : NORMAL;=0A=
};=0A=
=0A=
struct PixelInputType=0A=
{=0A=
    float4 position : SV_POSITION;=0A=
    float2 tex : TEXCOORD0;=0A=
    float3 normal : NORMAL;=0A=
    float4 viewPosition : TEXCOORD1;=0A=
    float3 lightPos : TEXCOORD2;=0A=
};=0A=
=0A=
=0A=
/////////////////////////////////////////////////////////////////////////=
///////=0A=
// Vertex Shader=0A=
/////////////////////////////////////////////////////////////////////////=
///////=0A=
PixelInputType SoftShadowVertexShader(VertexInputType input)=0A=
{=0A=
    PixelInputType output;=0A=
    float4 worldPosition;=0A=
    =0A=
    =0A=
    // Change the position vector to be 4 units for proper matrix =
calculations.=0A=
    input.position.w =3D 1.0f;=0A=
=0A=
    // Calculate the position of the vertex against the world, view, and =
projection matrices.=0A=
    output.position =3D mul(input.position, worldMatrix);=0A=
    output.position =3D mul(output.position, viewMatrix);=0A=
    output.position =3D mul(output.position, projectionMatrix);=0A=
</FONT></PRE>
<P align=3D"left"><FONT color=3D"#ffffff" face=3D"Arial" size=3D"2">The =
viewPosition=20
will be used to calculate the projection coordinates to project the soft =
shadows=20
onto the scene. </FONT></P>
<PRE><FONT color=3D"#aaffaa" face=3D"arial" size=3D"2">    // Store the =
position of the vertice as viewed by the camera in a separate variable.=0A=
    output.viewPosition =3D output.position;=0A=
=0A=
    // Store the texture coordinates for the pixel shader.=0A=
    output.tex =3D input.tex;=0A=
    =0A=
    // Calculate the normal vector against the world matrix only.=0A=
    output.normal =3D mul(input.normal, (float3x3)worldMatrix);=0A=
	=0A=
    // Normalize the normal vector.=0A=
    output.normal =3D normalize(output.normal);=0A=
=0A=
    // Calculate the position of the vertex in the world.=0A=
    worldPosition =3D mul(input.position, worldMatrix);=0A=
=0A=
    // Determine the light position based on the position of the light =
and the position of the vertex in the world.=0A=
    output.lightPos =3D lightPosition.xyz - worldPosition.xyz;=0A=
=0A=
    // Normalize the light position vector.=0A=
    output.lightPos =3D normalize(output.lightPos);=0A=
=0A=
    return output;=0A=
}=0A=
</FONT></PRE><BR>
<P align=3D"left"><FONT color=3D"#ffffff" face=3D"Arial"=20
size=3D"3"><U>Softshadow.ps</U></FONT></P>
<PRE><FONT color=3D"#aaffaa" face=3D"arial" =
size=3D"2">//////////////////////////////////////////////////////////////=
//////////////////=0A=
// Filename: softshadow.ps=0A=
/////////////////////////////////////////////////////////////////////////=
///////=0A=
=0A=
=0A=
//////////////=0A=
// TEXTURES //=0A=
//////////////=0A=
Texture2D shaderTexture : register(t0);=0A=
</FONT></PRE>
<P align=3D"left"><FONT color=3D"#ffffff" face=3D"Arial" size=3D"2">The =
shadowTexture is=20
the black and white blurred image that contains the soft shadows. We =
will=20
project this texture onto the scene. </FONT></P>
<PRE><FONT color=3D"#aaffaa" face=3D"arial" size=3D"2">Texture2D =
shadowTexture : register(t1);=0A=
=0A=
=0A=
///////////////////=0A=
// SAMPLE STATES //=0A=
///////////////////=0A=
SamplerState SampleTypeClamp : register(s0);=0A=
SamplerState SampleTypeWrap  : register(s1);=0A=
=0A=
=0A=
//////////////////////=0A=
// CONSTANT BUFFERS //=0A=
//////////////////////=0A=
cbuffer LightBuffer=0A=
{=0A=
    float4 ambientColor;=0A=
    float4 diffuseColor;=0A=
};=0A=
=0A=
=0A=
//////////////=0A=
// TYPEDEFS //=0A=
//////////////=0A=
struct PixelInputType=0A=
{=0A=
    float4 position : SV_POSITION;=0A=
    float2 tex : TEXCOORD0;=0A=
    float3 normal : NORMAL;=0A=
    float4 viewPosition : TEXCOORD1;=0A=
    float3 lightPos : TEXCOORD2;=0A=
};=0A=
=0A=
=0A=
/////////////////////////////////////////////////////////////////////////=
///////=0A=
// Pixel Shader=0A=
/////////////////////////////////////////////////////////////////////////=
///////=0A=
float4 SoftShadowPixelShader(PixelInputType input) : SV_TARGET=0A=
{=0A=
    float4 color;=0A=
    float lightIntensity;=0A=
    float4 textureColor;=0A=
    float2 projectTexCoord;=0A=
    float shadowValue;=0A=
</FONT></PRE>
<P align=3D"left"><FONT color=3D"#ffffff" face=3D"Arial" =
size=3D"2">Calculate the=20
lighting and texturing as normal.  </FONT></P>
<PRE><FONT color=3D"#aaffaa" face=3D"arial" size=3D"2">    // Set the =
default output color to the ambient light value for all pixels.=0A=
    color =3D ambientColor;=0A=
=0A=
    // Calculate the amount of light on this pixel.=0A=
    lightIntensity =3D saturate(dot(input.normal, input.lightPos));=0A=
    if(lightIntensity &gt; 0.0f)=0A=
    {=0A=
        // Determine the light color based on the diffuse color and the =
amount of light intensity.=0A=
        color +=3D (diffuseColor * lightIntensity);=0A=
=0A=
        // Saturate the light color.=0A=
        color =3D saturate(color);=0A=
    }=0A=
=0A=
    // Sample the pixel color from the texture using the sampler at this =
texture coordinate location.=0A=
    textureColor =3D shaderTexture.Sample(SampleTypeWrap, input.tex);=0A=
=0A=
    // Combine the light and texture color.=0A=
    color =3D color * textureColor;=0A=
</FONT></PRE>
<P align=3D"left"><FONT color=3D"#ffffff" face=3D"Arial" size=3D"2">Now =
calculate the=20
projected coordinates to sample the blurred black and white image from =
based on=20
the camera's view point. </FONT></P>
<PRE><FONT color=3D"#aaffaa" face=3D"arial" size=3D"2">    // Calculate =
the projected texture coordinates to be used with the shadow texture.=0A=
    projectTexCoord.x =3D  input.viewPosition.x / input.viewPosition.w / =
2.0f + 0.5f;=0A=
    projectTexCoord.y =3D -input.viewPosition.y / input.viewPosition.w / =
2.0f + 0.5f;=0A=
</FONT></PRE>
<P align=3D"left"><FONT color=3D"#ffffff" face=3D"Arial" =
size=3D"2">Sample the soft=20
shadow image using the projected coordinates and then multiple it in the =
same=20
fashion as a light map to the final color to get the soft shadow effect. =

</FONT></P>
<PRE><FONT color=3D"#aaffaa" face=3D"arial" size=3D"2">    // Sample the =
shadow value from the shadow texture using the sampler at the projected =
texture coordinate location.=0A=
    shadowValue =3D shadowTexture.Sample(SampleTypeClamp, =
projectTexCoord).r;=0A=
=0A=
    // Combine the shadows with the final color.=0A=
    color =3D color * shadowValue;=0A=
=0A=
    return color;=0A=
}=0A=
</FONT></PRE><BR>
