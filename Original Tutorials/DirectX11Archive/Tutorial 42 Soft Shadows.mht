/////////////=0A=
cbuffer MatrixBuffer=0A=
{=0A=
    matrix worldMatrix;=0A=
    matrix viewMatrix;=0A=
    matrix projectionMatrix;=0A=
    matrix lightViewMatrix;=0A=
    matrix lightProjectionMatrix;=0A=
};=0A=
=0A=
=0A=
//////////////////////=0A=
// CONSTANT BUFFERS //=0A=
//////////////////////=0A=
cbuffer LightBuffer2=0A=
{=0A=
    float3 lightPosition;=0A=
    float padding;=0A=
};=0A=
=0A=
=0A=
//////////////=0A=
// TYPEDEFS //=0A=
//////////////=0A=
struct VertexInputType=0A=
{=0A=
    float4 position : POSITION;=0A=
    float2 tex : TEXCOORD0;=0A=
    float3 normal : NORMAL;=0A=
};=0A=
=0A=
struct PixelInputType=0A=
{=0A=
    float4 position : SV_POSITION;=0A=
    float2 tex : TEXCOORD0;=0A=
    float3 normal : NORMAL;=0A=
    float4 lightViewPosition : TEXCOORD1;=0A=
    float3 lightPos : TEXCOORD2;=0A=
