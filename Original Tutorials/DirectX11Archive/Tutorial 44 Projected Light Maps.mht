=0A=
	// Get the world, view, and projection matrices from the camera and d3d =
objects.=0A=
	m_D3D-&gt;GetWorldMatrix(worldMatrix);=0A=
	m_Camera-&gt;GetViewMatrix(viewMatrix);=0A=
	m_D3D-&gt;GetProjectionMatrix(projectionMatrix);=0A=
=0A=
	// Get the view and projection matrices from the view point object.=0A=
	m_ViewPoint-&gt;GetViewMatrix(viewMatrix2);=0A=
	m_ViewPoint-&gt;GetProjectionMatrix(projectionMatrix2);=0A=
=0A=
	// Setup the translation for the ground model.=0A=
	D3DXMatrixTranslation(&amp;worldMatrix, 0.0f, 1.0f, 0.0f);=0A=
	=0A=
	// Render the ground model using the projection shader.=0A=
	m_GroundModel-&gt;Render(m_D3D-&gt;GetDeviceContext());=0A=
	result =3D m_ProjectionShader-&gt;Render(m_D3D-&gt;GetDeviceContext(), =
m_GroundModel-&gt;GetIndexCount(), worldMatrix, viewMatrix, =
projectionMatrix, =0A=
					    m_GroundModel-&gt;GetTexture(), m_Light-&gt;GetAmbientColor(), =
m_Light-&gt;GetDiffuseColor(), m_Light-&gt;GetPosition(), =0A=
					    viewMatrix2, projectionMatrix2, =
m_ProjectionTexture-&gt;GetTexture());=0A=
	if(!result)=0A=
	{=0A=
		return false;=0A=
	}=0A=
=0A=
	// Reset the world matrix and setup the translation for the cube model.=0A=
	m_D3D-&gt;GetWorldMatrix(worldMatrix);=0A=
	D3DXMatrixTranslation(&amp;worldMatrix, 0.0f, 2.0f, 0.0f);=0A=
=0A=
	// Render the cube model using the projection shader.=0A=
	m_CubeModel-&gt;Render(m_D3D-&gt;GetDeviceContext());=0A=
	result =3D m_ProjectionShader-&gt;Render(m_D3D-&gt;GetDeviceContext(), =
m_CubeModel-&gt;GetIndexCount(), worldMatrix, viewMatrix, =
projectionMatrix, =0A=
					    m_CubeModel-&gt;GetTexture(), m_Light-&gt;GetAmbientColor(), =
m_Light-&gt;GetDiffuseColor(), m_Light-&gt;GetPosition(), =0A=
					    viewMatrix2, projectionMatrix2, =
m_ProjectionTexture-&gt;GetTexture());=0A=
	if(!result)=0A=
	{=0A=
		return false;=0A=
	}=0A=
							   =0A=
	// Present the rendered scene to the screen.=0A=
	m_D3D-&gt;EndScene();=0A=
=0A=
	return true;=0A=
}=0A=
