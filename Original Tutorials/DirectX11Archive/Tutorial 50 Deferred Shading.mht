bool DeferredBuffersClass::Initialize(ID3D11Device* device, int =
textureWidth, int textureHeight, float screenDepth, float screenNear)=0A=
{=0A=
	D3D11_TEXTURE2D_DESC textureDesc;=0A=
	HRESULT result;=0A=
	D3D11_RENDER_TARGET_VIEW_DESC renderTargetViewDesc;=0A=
	D3D11_SHADER_RESOURCE_VIEW_DESC shaderResourceViewDesc;=0A=
	D3D11_TEXTURE2D_DESC depthBufferDesc;=0A=
	D3D11_DEPTH_STENCIL_VIEW_DESC depthStencilViewDesc;=0A=
	int i;=0A=
=0A=
=0A=
	// Store the width and height of the render texture.=0A=
	m_textureWidth =3D textureWidth;=0A=
	m_textureHeight =3D textureHeight;=0A=
=0A=
	// Initialize the render target texture description.=0A=
	ZeroMemory(&amp;textureDesc, sizeof(textureDesc));=0A=
=0A=
	// Setup the render target texture description.=0A=
	textureDesc.Width =3D textureWidth;=0A=
	textureDesc.Height =3D textureHeight;=0A=
	textureDesc.MipLevels =3D 1;=0A=
	textureDesc.ArraySize =3D 1;=0A=
	textureDesc.Format =3D DXGI_FORMAT_R32G32B32A32_FLOAT;=0A=
	textureDesc.SampleDesc.Count =3D 1;=0A=
	textureDesc.Usage =3D D3D11_USAGE_DEFAULT;=0A=
	textureDesc.BindFlags =3D D3D11_BIND_RENDER_TARGET | =
D3D11_BIND_SHADER_RESOURCE;=0A=
	textureDesc.CPUAccessFlags =3D 0;=0A=
	textureDesc.MiscFlags =3D 0;=0A=
</FONT></PRE>
<P align=3D"left"><FONT color=3D"#ffffff" face=3D"Arial" size=3D"2">We =
now create an=20
array of render target textures for the deferred shader to write to. =
</FONT></P>
<PRE><FONT color=3D"#aaffaa" face=3D"arial" size=3D"2">	// Create the =
render target textures.=0A=
	for(i=3D0; i&lt;BUFFER_COUNT; i++)=0A=
	{=0A=
		result =3D device-&gt;CreateTexture2D(&amp;textureDesc, NULL, =
&amp;m_renderTargetTextureArray[i]);=0A=
		if(FAILED(result))=0A=
		{=0A=
			return false;=0A=
		}=0A=
	}=0A=
=0A=
	// Setup the description of the render target view.=0A=
	renderTargetViewDesc.Format =3D textureDesc.Format;=0A=
	renderTargetViewDesc.ViewDimension =3D D3D11_RTV_DIMENSION_TEXTURE2D;=0A=
	renderTargetViewDesc.Texture2D.MipSlice =3D 0;=0A=
</FONT></PRE>
<P align=3D"left"><FONT color=3D"#ffffff" face=3D"Arial" size=3D"2">We =
also create an=20
array of render target views to access the render target textures =
through.=20
</FONT></P>
<PRE><FONT color=3D"#aaffaa" face=3D"arial" size=3D"2">	// Create the =
render target views.=0A=
	for(i=3D0; i&lt;BUFFER_COUNT; i++)=0A=
	{=0A=
		result =3D =
device-&gt;CreateRenderTargetView(m_renderTargetTextureArray[i], =
&amp;renderTargetViewDesc, &amp;m_renderTargetViewArray[i]);=0A=
		if(FAILED(result))=0A=
		{=0A=
			return false;=0A=
		}=0A=
	}=0A=
=0A=
	// Setup the description of the shader resource view.=0A=
	shaderResourceViewDesc.Format =3D textureDesc.Format;=0A=
	shaderResourceViewDesc.ViewDimension =3D D3D11_SRV_DIMENSION_TEXTURE2D;=0A=
	shaderResourceViewDesc.Texture2D.MostDetailedMip =3D 0;=0A=
	shaderResourceViewDesc.Texture2D.MipLevels =3D 1;=0A=
</FONT></PRE>
<P align=3D"left"><FONT color=3D"#ffffff" face=3D"Arial" size=3D"2">We =
also create an=20
array of shader resource views for the shaders to access the render =
target=20
textures. </FONT></P>
<PRE><FONT color=3D"#aaffaa" face=3D"arial" size=3D"2">	// Create the =
shader resource views.=0A=
	for(i=3D0; i&lt;BUFFER_COUNT; i++)=0A=
	{=0A=
		result =3D =
device-&gt;CreateShaderResourceView(m_renderTargetTextureArray[i], =
&amp;shaderResourceViewDesc, &amp;m_shaderResourceViewArray[i]);=0A=
		if(FAILED(result))=0A=
		{=0A=
			return false;=0A=
		}=0A=
	}=0A=
=0A=
	// Initialize the description of the depth buffer.=0A=
	ZeroMemory(&amp;depthBufferDesc, sizeof(depthBufferDesc));=0A=
=0A=
	// Set up the description of the depth buffer.=0A=
	depthBufferDesc.Width =3D textureWidth;=0A=
	depthBufferDesc.Height =3D textureHeight;=0A=
	depthBufferDesc.MipLevels =3D 1;=0A=
	depthBufferDesc.ArraySize =3D 1;=0A=
	depthBufferDesc.Format =3D DXGI_FORMAT_D24_UNORM_S8_UINT;=0A=
	depthBufferDesc.SampleDesc.Count =3D 1;=0A=
	depthBufferDesc.SampleDesc.Quality =3D 0;=0A=
	depthBufferDesc.Usage =3D D3D11_USAGE_DEFAULT;=0A=
	depthBufferDesc.BindFlags =3D D3D11_BIND_DEPTH_STENCIL;=0A=
	depthBufferDesc.CPUAccessFlags =3D 0;=0A=
	depthBufferDesc.MiscFlags =3D 0;=0A=
=0A=
	// Create the texture for the depth buffer using the filled out =
description.=0A=
	result =3D device-&gt;CreateTexture2D(&amp;depthBufferDesc, NULL, =
&amp;m_depthStencilBuffer);=0A=
	if(FAILED(result))=0A=
	{=0A=
		return false;=0A=
	}=0A=
=0A=
	// Initailze the depth stencil view description.=0A=
	ZeroMemory(&amp;depthStencilViewDesc, sizeof(depthStencilViewDesc));=0A=
=0A=
	// Set up the depth stencil view description.=0A=
	depthStencilViewDesc.Format =3D DXGI_FORMAT_D24_UNORM_S8_UINT;=0A=
	depthStencilViewDesc.ViewDimension =3D D3D11_DSV_DIMENSION_TEXTURE2D;=0A=
	depthStencilViewDesc.Texture2D.MipSlice =3D 0;=0A=
=0A=
	// Create the depth stencil view.=0A=
	result =3D device-&gt;CreateDepthStencilView(m_depthStencilBuffer, =
&amp;depthStencilViewDesc, &amp;m_depthStencilView);=0A=
	if(FAILED(result))=0A=
	{=0A=
		return false;=0A=
	}=0A=
=0A=
	// Setup the viewport for rendering.=0A=
	m_viewport.Width =3D (float)textureWidth;=0A=
	m_viewport.Height =3D (float)textureHeight;=0A=
	m_viewport.MinDepth =3D 0.0f;=0A=
	m_viewport.MaxDepth =3D 1.0f;=0A=
	m_viewport.TopLeftX =3D 0.0f;=0A=
	m_viewport.TopLeftY =3D 0.0f;=0A=
=0A=
	return true;=0A=
}=0A=
=0A=
=0A=
void DeferredBuffersClass::Shutdown()=0A=
{=0A=
	int i;=0A=
=0A=
=0A=
	if(m_depthStencilView)=0A=
	{=0A=
