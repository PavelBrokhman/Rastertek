		m_depthStencilView =3D 0;=0A=
	}=0A=
=0A=
	if(m_depthStencilBuffer)=0A=
	{=0A=
		m_depthStencilBuffer-&gt;Release();=0A=
		m_depthStencilBuffer =3D 0;=0A=
	}=0A=
=0A=
	for(i=3D0; i&lt;BUFFER_COUNT; i++)=0A=
	{=0A=
		if(m_shaderResourceViewArray[i])=0A=
		{=0A=
			m_shaderResourceViewArray[i]-&gt;Release();=0A=
			m_shaderResourceViewArray[i] =3D 0;=0A=
		}=0A=
=0A=
		if(m_renderTargetViewArray[i])=0A=
		{=0A=
