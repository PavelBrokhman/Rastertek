void DeferredShaderClass::OutputShaderErrorMessage(ID3D10Blob* =
errorMessage, HWND hwnd, WCHAR* shaderFilename)=0A=
{=0A=
	char* compileErrors;=0A=
	unsigned long bufferSize, i;=0A=
	ofstream fout;=0A=
