}=0A=
=0A=
=0A=
void ModelClass::ReleaseTexture()=0A=
{=0A=
	// Release the texture object.=0A=
	if(m_Texture)=0A=
	{=0A=
		m_Texture-&gt;Shutdown();=0A=
		delete m_Texture;=0A=
		m_Texture =3D 0;=0A=
	}=0A=
=0A=
	return;=0A=
}=0A=
</FONT></PRE>
<P align=3D"left"><FONT color=3D"#ffffff" face=3D"Arial" size=3D"2">This =
is the new=20
LoadModel function which handles loading the model data from the text =
file into=20
the m_model array variable.  It opens the text file and reads in the =
vertex=20
count first.  After reading the vertex count it creates the ModelType =
array and=20
then reads each line into the array.  Both the vertex count and index =
count are=20
now set in this function. </FONT></P>
<PRE><FONT color=3D"#aaaaff" face=3D"arial" size=3D"2">bool =
ModelClass::LoadModel(char* filename)=0A=
{=0A=
	ifstream fin;=0A=
	char input;=0A=
	int i;=0A=
=0A=
=0A=
	// Open the model file.=0A=
	fin.open(filename);=0A=
	=0A=
	// If it could not open the file then exit.=0A=
	if(fin.fail())=0A=
	{=0A=
		return false;=0A=
	}=0A=
=0A=
	// Read up to the value of vertex count.=0A=
	fin.get(input);=0A=
	while(input !=3D ':')=0A=
	{=0A=
		fin.get(input);=0A=
	}=0A=
=0A=
	// Read in the vertex count.=0A=
	fin &gt;&gt; m_vertexCount;=0A=
=0A=
	// Set the number of indices to be the same as the vertex count.=0A=
	m_indexCount =3D m_vertexCount;=0A=
=0A=
	// Create the model using the vertex count that was read in.=0A=
	m_model =3D new ModelType[m_vertexCount];=0A=
	if(!m_model)=0A=
	{=0A=
		return false;=0A=
	}=0A=
=0A=
	// Read up to the beginning of the data.=0A=
	fin.get(input);=0A=
	while(input !=3D ':')=0A=
	{=0A=
		fin.get(input);=0A=
	}=0A=
	fin.get(input);=0A=
	fin.get(input);=0A=
=0A=
	// Read in the vertex data.=0A=
	for(i=3D0; i&lt;m_vertexCount; i++)=0A=
	{=0A=
		fin &gt;&gt; m_model[i].x &gt;&gt; m_model[i].y &gt;&gt; m_model[i].z;=0A=
		fin &gt;&gt; m_model[i].tu &gt;&gt; m_model[i].tv;=0A=
		fin &gt;&gt; m_model[i].nx &gt;&gt; m_model[i].ny &gt;&gt; =
m_model[i].nz;=0A=
	}=0A=
=0A=
	// Close the model file.=0A=
	fin.close();=0A=
=0A=
	return true;=0A=
}=0A=
