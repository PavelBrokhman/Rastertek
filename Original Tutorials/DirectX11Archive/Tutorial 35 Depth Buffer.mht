	ID3D11InputLayout* m_layout;=0A=
</FONT></PRE>
<P align=3D"left"><FONT color=3D"#ffffff" face=3D"Arial" size=3D"2">We =
only require the=20
matrix constant buffer. </FONT></P>
<PRE><FONT color=3D"#aaffaa" face=3D"arial" size=3D"2">	ID3D11Buffer* =
m_matrixBuffer;=0A=
};=0A=
=0A=
#endif=0A=
</FONT></PRE><BR>
<P align=3D"left"><FONT color=3D"#ffffff" face=3D"Arial"=20
size=3D"3"><U>Depthshaderclass.cpp</U></FONT></P>
<PRE><FONT color=3D"#aaffaa" face=3D"arial" =
size=3D"2">//////////////////////////////////////////////////////////////=
//////////////////=0A=
// Filename: depthshaderclass.cpp=0A=
/////////////////////////////////////////////////////////////////////////=
///////=0A=
#include "depthshaderclass.h"=0A=
</FONT></PRE>
<P align=3D"left"><FONT color=3D"#ffffff" face=3D"Arial" size=3D"2">The =
class=20
constructor initializes all the private pointers to null. </FONT></P>
<PRE><FONT color=3D"#aaffaa" face=3D"arial" =
size=3D"2">DepthShaderClass::DepthShaderClass()=0A=
{=0A=
	m_vertexShader =3D 0;=0A=
	m_pixelShader =3D 0;=0A=
	m_layout =3D 0;=0A=
	m_matrixBuffer =3D 0;=0A=
}=0A=
=0A=
=0A=
DepthShaderClass::DepthShaderClass(const DepthShaderClass&amp; other)=0A=
{=0A=
}=0A=
=0A=
=0A=
DepthShaderClass::~DepthShaderClass()=0A=
{=0A=
}=0A=
=0A=
=0A=
bool DepthShaderClass::Initialize(ID3D11Device* device, HWND hwnd)=0A=
{=0A=
	bool result;=0A=
</FONT></PRE>
<P align=3D"left"><FONT color=3D"#ffffff" face=3D"Arial" size=3D"2">We =
load the depth.fx=20
HLSL shader here. </FONT></P>
<PRE><FONT color=3D"#aaffaa" face=3D"arial" size=3D"2">	// Initialize =
the vertex and pixel shaders.=0A=
	result =3D InitializeShader(device, hwnd, L"../Engine/depth.vs", =
L"../Engine/depth.ps");=0A=
	if(!result)=0A=
	{=0A=
		return false;=0A=
	}=0A=
=0A=
	return true;=0A=
}=0A=
=0A=
=0A=
void DepthShaderClass::Shutdown()=0A=
{=0A=
	// Shutdown the vertex and pixel shaders as well as the related objects.=0A=
