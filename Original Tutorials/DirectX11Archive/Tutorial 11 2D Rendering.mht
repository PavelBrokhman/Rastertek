	}=0A=
=0A=
	return;=0A=
}=0A=
=0A=
=0A=
void D3DClass::BeginScene(float red, float green, float blue, float =
alpha)=0A=
{=0A=
	float color[4];=0A=
=0A=
=0A=
	// Setup the color to clear the buffer to.=0A=
	color[0] =3D red;=0A=
	color[1] =3D green;=0A=
