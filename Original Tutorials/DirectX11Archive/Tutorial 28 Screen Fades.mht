=0A=
bool FadeShaderClass::SetShaderParameters(ID3D11DeviceContext* =
deviceContext, D3DXMATRIX worldMatrix, D3DXMATRIX viewMatrix,=0A=
					  D3DXMATRIX projectionMatrix, ID3D11ShaderResourceView* texture, =
float fadeAmount)=0A=
{=0A=
	HRESULT result;=0A=
	D3D11_MAPPED_SUBRESOURCE mappedResource;=0A=
	MatrixBufferType* dataPtr;=0A=
	unsigned int bufferNumber;=0A=
	FadeBufferType* dataPtr2;=0A=
=0A=
=0A=
	// Transpose the matrices to prepare them for the shader.=0A=
	D3DXMatrixTranspose(&amp;worldMatrix, &amp;worldMatrix);=0A=
	D3DXMatrixTranspose(&amp;viewMatrix, &amp;viewMatrix);=0A=
	D3DXMatrixTranspose(&amp;projectionMatrix, &amp;projectionMatrix);=0A=
=0A=
	// Lock the matrix constant buffer so it can be written to.=0A=
	result =3D deviceContext-&gt;Map(m_matrixBuffer, 0, =
D3D11_MAP_WRITE_DISCARD, 0, &amp;mappedResource);=0A=
	if(FAILED(result))=0A=
	{=0A=
		return false;=0A=
	}=0A=
=0A=
	// Get a pointer to the data in the matrix constant buffer.=0A=
	dataPtr =3D (MatrixBufferType*)mappedResource.pData;=0A=
=0A=
	// Copy the matrices into the matrix constant buffer.=0A=
	dataPtr-&gt;world =3D worldMatrix;=0A=
	dataPtr-&gt;view =3D viewMatrix;=0A=
	dataPtr-&gt;projection =3D projectionMatrix;=0A=
=0A=
	// Unlock the matrix constant buffer.=0A=
	deviceContext-&gt;Unmap(m_matrixBuffer, 0);=0A=
=0A=
	// Set the position of the matrix constant buffer in the vertex shader.=0A=
	bufferNumber =3D 0;=0A=
=0A=
	// Now set the matrix constant buffer in the vertex shader with the =
updated values.=0A=
	deviceContext-&gt;VSSetConstantBuffers(bufferNumber, 1, =
&amp;m_matrixBuffer);=0A=
=0A=
	// Set shader texture resource in the pixel shader.=0A=
	deviceContext-&gt;PSSetShaderResources(0, 1, &amp;texture);=0A=
</FONT></PRE>
<P align=3D"left"><FONT color=3D"#ffffff" face=3D"Arial" size=3D"2">This =
is where we set=20
the fadeAmount value inside the pixel shader. We first lock the fade =
buffer and=20
then copy the updated fadeAmount value into it. After that we unlock the =
fade=20
buffer and then set it in the pixel shader using the =
PSSetConstantBuffers=20
function. </FONT></P>
<PRE><FONT color=3D"#aaffaa" face=3D"arial" size=3D"2">	// Lock the fade =
constant buffer so it can be written to.=0A=
	result =3D deviceContext-&gt;Map(m_fadeBuffer, 0, =
D3D11_MAP_WRITE_DISCARD, 0, &amp;mappedResource);=0A=
	if(FAILED(result))=0A=
	{=0A=
		return false;=0A=
	}=0A=
=0A=
	// Get a pointer to the data in the fade constant buffer.=0A=
	dataPtr2 =3D (FadeBufferType*)mappedResource.pData;=0A=
=0A=
	// Copy the fade amount into the fade constant buffer.=0A=
	dataPtr2-&gt;fadeAmount =3D fadeAmount;=0A=
	dataPtr2-&gt;padding =3D D3DXVECTOR3(0.0f, 0.0f, 0.0f);=0A=
=0A=
	// Unlock the fade constant buffer.=0A=
	deviceContext-&gt;Unmap(m_fadeBuffer, 0);=0A=
=0A=
	// Set the position of the fade constant buffer in the pixel shader.=0A=
	bufferNumber =3D 0;=0A=
=0A=
	// Now set the fade constant buffer in the pixel shader with the =
updated values.=0A=
	deviceContext-&gt;PSSetConstantBuffers(bufferNumber, 1, =
&amp;m_fadeBuffer);=0A=
=0A=
	return true;=0A=
}=0A=
=0A=
=0A=
void FadeShaderClass::RenderShader(ID3D11DeviceContext* deviceContext, =
int indexCount)=0A=
{=0A=
	// Set the vertex input layout.=0A=
	deviceContext-&gt;IASetInputLayout(m_layout);=0A=
=0A=
	// Set the vertex and pixel shaders that will be used to render this =
triangle.=0A=
	deviceContext-&gt;VSSetShader(m_vertexShader, NULL, 0);=0A=
	deviceContext-&gt;PSSetShader(m_pixelShader, NULL, 0);=0A=
=0A=
	// Set the sampler state in the pixel shader.=0A=
	deviceContext-&gt;PSSetSamplers(0, 1, &amp;m_sampleState);=0A=
=0A=
	// Render the triangle.=0A=
	deviceContext-&gt;DrawIndexed(indexCount, 0, 0);=0A=
=0A=
	return;=0A=
}=0A=
</FONT></PRE><BR>
