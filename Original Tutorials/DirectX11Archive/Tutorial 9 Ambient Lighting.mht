	}=0A=
=0A=
	// Create the vertex shader from the buffer.=0A=
	result =3D =
device-&gt;CreateVertexShader(vertexShaderBuffer-&gt;GetBufferPointer(), =
vertexShaderBuffer-&gt;GetBufferSize(), NULL, &amp;m_vertexShader);=0A=
	if(FAILED(result))=0A=
	{=0A=
