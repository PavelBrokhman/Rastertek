	result =3D device-&gt;CreateBuffer(&amp;lightBufferDesc, NULL, =
&amp;m_lightBuffer);=0A=
	if(FAILED(result))=0A=
	{=0A=
		return false;=0A=
	}=0A=
=0A=
	return true;=0A=
}=0A=
=0A=
=0A=
void LightShaderClass::ShutdownShader()=0A=
