{=0A=
}=0A=
=0A=
=0A=
bool WaterShaderClass::Initialize(ID3D11Device* device, HWND hwnd)=0A=
{=0A=
	bool result;=0A=
</FONT></PRE>
<P align=3D"left"><FONT color=3D"#ffffff" face=3D"Arial" size=3D"2">Load =
the new=20
water.vs and water.ps HLSL shader files. </FONT></P>
<PRE><FONT color=3D"#aaffaa" face=3D"arial" size=3D"2">	// Initialize =
the vertex and pixel shaders.=0A=
	result =3D InitializeShader(device, hwnd, L"../Engine/water.vs", =
L"../Engine/water.ps");=0A=
