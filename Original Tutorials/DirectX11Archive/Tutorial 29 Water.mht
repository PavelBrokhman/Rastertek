=0A=
bool RefractionShaderClass::SetShaderParameters(ID3D11DeviceContext* =
deviceContext, D3DXMATRIX worldMatrix, D3DXMATRIX viewMatrix,=0A=
						D3DXMATRIX projectionMatrix, ID3D11ShaderResourceView* texture,=0A=
						D3DXVECTOR3 lightDirection, D3DXVECTOR4 ambientColor, D3DXVECTOR4 =
diffuseColor,=0A=
						D3DXVECTOR4 clipPlane)=0A=
{=0A=
	HRESULT result;=0A=
	D3D11_MAPPED_SUBRESOURCE mappedResource;=0A=
	MatrixBufferType* dataPtr;=0A=
	LightBufferType* dataPtr2;=0A=
	unsigned int bufferNumber;=0A=
	ClipPlaneBufferType* dataPtr3;=0A=
=0A=
=0A=
	// Set shader texture resource in the pixel shader.=0A=
	deviceContext-&gt;PSSetShaderResources(0, 1, &amp;texture);=0A=
=0A=
	// Lock the matrix constant buffer so it can be written to.=0A=
	result =3D deviceContext-&gt;Map(m_matrixBuffer, 0, =
D3D11_MAP_WRITE_DISCARD, 0, &amp;mappedResource);=0A=
	if(FAILED(result))=0A=
	{=0A=
		return false;=0A=
	}=0A=
=0A=
	// Get a pointer to the data in the constant buffer.=0A=
	dataPtr =3D (MatrixBufferType*)mappedResource.pData;=0A=
=0A=
	// Transpose the matrices to prepare them for the shader.=0A=
	D3DXMatrixTranspose(&amp;worldMatrix, &amp;worldMatrix);=0A=
	D3DXMatrixTranspose(&amp;viewMatrix, &amp;viewMatrix);=0A=
	D3DXMatrixTranspose(&amp;projectionMatrix, &amp;projectionMatrix);=0A=
=0A=
	// Copy the matrices into the constant buffer.=0A=
	dataPtr-&gt;world =3D worldMatrix;=0A=
	dataPtr-&gt;view =3D viewMatrix;=0A=
	dataPtr-&gt;projection =3D projectionMatrix;=0A=
=0A=
	// Unlock the matrix constant buffer.=0A=
	deviceContext-&gt;Unmap(m_matrixBuffer, 0);=0A=
=0A=
	// Set the position of the matrix constant buffer in the vertex shader.=0A=
	bufferNumber =3D 0;=0A=
=0A=
	// Now set the matrix constant buffer in the vertex shader with the =
updated values.=0A=
	deviceContext-&gt;VSSetConstantBuffers(bufferNumber, 1, =
&amp;m_matrixBuffer);=0A=
=0A=
	// Lock the light constant buffer so it can be written to.=0A=
	result =3D deviceContext-&gt;Map(m_lightBuffer, 0, =
D3D11_MAP_WRITE_DISCARD, 0, &amp;mappedResource);=0A=
	if(FAILED(result))=0A=
	{=0A=
		return false;=0A=
	}=0A=
=0A=
	// Get a pointer to the data in the constant buffer.=0A=
	dataPtr2 =3D (LightBufferType*)mappedResource.pData;=0A=
=0A=
	// Copy the lighting variables into the constant buffer.=0A=
	dataPtr2-&gt;ambientColor =3D ambientColor;=0A=
	dataPtr2-&gt;diffuseColor =3D diffuseColor;=0A=
	dataPtr2-&gt;lightDirection =3D lightDirection;=0A=
=0A=
	// Unlock the constant buffer.=0A=
	deviceContext-&gt;Unmap(m_lightBuffer, 0);=0A=
=0A=
	// Set the position of the light constant buffer in the pixel shader.=0A=
	bufferNumber =3D 0;=0A=
=0A=
	// Finally set the light constant buffer in the pixel shader with the =
updated values.=0A=
	deviceContext-&gt;PSSetConstantBuffers(bufferNumber, 1, =
&amp;m_lightBuffer);=0A=
</FONT></PRE>
<P align=3D"left"><FONT color=3D"#ffffff" face=3D"Arial" size=3D"2">Set =
the clip plane=20
inside the vertex shader. </FONT></P>
<PRE><FONT color=3D"#aaffaa" face=3D"arial" size=3D"2">	// Lock the clip =
plane constant buffer so it can be written to.=0A=
	result =3D deviceContext-&gt;Map(m_clipPlaneBuffer, 0, =
D3D11_MAP_WRITE_DISCARD, 0, &amp;mappedResource);=0A=
	if(FAILED(result))=0A=
	{=0A=
		return false;=0A=
	}=0A=
=0A=
	// Get a pointer to the data in the clip plane constant buffer.=0A=
	dataPtr3 =3D (ClipPlaneBufferType*)mappedResource.pData;=0A=
=0A=
	// Copy the clip plane into the clip plane constant buffer.=0A=
	dataPtr3-&gt;clipPlane =3D clipPlane;=0A=
=0A=
	// Unlock the buffer.=0A=
	deviceContext-&gt;Unmap(m_clipPlaneBuffer, 0);=0A=
=0A=
	// Set the position of the clip plane constant buffer in the vertex =
shader.=0A=
	bufferNumber =3D 1;=0A=
=0A=
	// Now set the clip plane constant buffer in the vertex shader with the =
updated values.=0A=
	deviceContext-&gt;VSSetConstantBuffers(bufferNumber, 1, =
&amp;m_clipPlaneBuffer);=0A=
=0A=
	return true;=0A=
}=0A=
