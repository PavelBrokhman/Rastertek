	ID3D11InputLayout* m_layout;=0A=
	ID3D11SamplerState* m_sampleState;=0A=
	ID3D11Buffer* m_matrixBuffer;=0A=
</FONT></PRE>
<P align=3D"left"><FONT color=3D"#ffffff" face=3D"Arial" size=3D"2">We =
have two new=20
buffers for the new reflection and water constant buffers that are =
inside the=20
vertex and pixel shader.  </FONT></P>
<PRE><FONT color=3D"#aaffaa" face=3D"arial" size=3D"2">	ID3D11Buffer* =
m_reflectionBuffer;=0A=
	ID3D11Buffer* m_waterBuffer;=0A=
};=0A=
=0A=
#endif=0A=
</FONT></PRE><BR>
<P align=3D"left"><FONT color=3D"#ffffff" face=3D"Arial"=20
size=3D"3"><U>Watershaderclass.cpp</U></FONT></P>
<PRE><FONT color=3D"#aaffaa" face=3D"arial" =
size=3D"2">//////////////////////////////////////////////////////////////=
//////////////////=0A=
// Filename: watershaderclass.cpp=0A=
/////////////////////////////////////////////////////////////////////////=
///////=0A=
#include "watershaderclass.h"=0A=
=0A=
=0A=
WaterShaderClass::WaterShaderClass()=0A=
{=0A=
	m_vertexShader =3D 0;=0A=
	m_pixelShader =3D 0;=0A=
	m_layout =3D 0;=0A=
	m_sampleState =3D 0;=0A=
	m_matrixBuffer =3D 0;=0A=
</FONT></PRE>
<P align=3D"left"><FONT color=3D"#ffffff" face=3D"Arial" =
size=3D"2">Initialize the new=20
pointers inside the class constructor. </FONT></P>
<PRE><FONT color=3D"#aaffaa" face=3D"arial" size=3D"2">	=
m_reflectionBuffer =3D 0;=0A=
	m_waterBuffer =3D 0;=0A=
}=0A=
=0A=
=0A=
WaterShaderClass::WaterShaderClass(const WaterShaderClass&amp; other)=0A=
{=0A=
