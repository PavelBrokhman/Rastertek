		{=0A=
			m_leftTurnSpeed =3D 0.0f;=0A=
		}=0A=
	}=0A=
=0A=
	// Update the rotation using the turning speed.=0A=
	m_rotationY -=3D m_leftTurnSpeed;=0A=
