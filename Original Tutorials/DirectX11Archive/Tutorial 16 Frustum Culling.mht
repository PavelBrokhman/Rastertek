<P align=3D"left"><FONT color=3D"#ffffff" face=3D"Arial" =
size=3D"2">ConstructFrustum is=20
called every frame by the GraphicsClass.  It passes in the the depth of =
the=20
screen, the projection matrix, and the view matrix.  We then use these =
input=20
variables to calculate the matrix of the view frustum at that frame.  =
With the=20
new frustum matrix we then calculate the six planes that form the view =
frustum.=20
</FONT></P>
<PRE><FONT color=3D"#aaffaa" face=3D"arial" size=3D"2">void =
FrustumClass::ConstructFrustum(float screenDepth, D3DXMATRIX =
projectionMatrix, D3DXMATRIX viewMatrix)=0A=
{=0A=
	float zMinimum, r;=0A=
	D3DXMATRIX matrix;=0A=
=0A=
	=0A=
	// Calculate the minimum Z distance in the frustum.=0A=
	zMinimum =3D -projectionMatrix._43 / projectionMatrix._33;=0A=
	r =3D screenDepth / (screenDepth - zMinimum);=0A=
	projectionMatrix._33 =3D r;=0A=
	projectionMatrix._43 =3D -r * zMinimum;=0A=
=0A=
	// Create the frustum matrix from the view matrix and updated =
projection matrix.=0A=
	D3DXMatrixMultiply(&amp;matrix, &amp;viewMatrix, &amp;projectionMatrix);=0A=
=0A=
	// Calculate near plane of frustum.=0A=
	m_planes[0].a =3D matrix._14 + matrix._13;=0A=
	m_planes[0].b =3D matrix._24 + matrix._23;=0A=
	m_planes[0].c =3D matrix._34 + matrix._33;=0A=
	m_planes[0].d =3D matrix._44 + matrix._43;=0A=
	D3DXPlaneNormalize(&amp;m_planes[0], &amp;m_planes[0]);=0A=
=0A=
	// Calculate far plane of frustum.=0A=
	m_planes[1].a =3D matrix._14 - matrix._13; =0A=
	m_planes[1].b =3D matrix._24 - matrix._23;=0A=
	m_planes[1].c =3D matrix._34 - matrix._33;=0A=
	m_planes[1].d =3D matrix._44 - matrix._43;=0A=
	D3DXPlaneNormalize(&amp;m_planes[1], &amp;m_planes[1]);=0A=
=0A=
	// Calculate left plane of frustum.=0A=
	m_planes[2].a =3D matrix._14 + matrix._11; =0A=
	m_planes[2].b =3D matrix._24 + matrix._21;=0A=
	m_planes[2].c =3D matrix._34 + matrix._31;=0A=
	m_planes[2].d =3D matrix._44 + matrix._41;=0A=
	D3DXPlaneNormalize(&amp;m_planes[2], &amp;m_planes[2]);=0A=
=0A=
	// Calculate right plane of frustum.=0A=
	m_planes[3].a =3D matrix._14 - matrix._11; =0A=
	m_planes[3].b =3D matrix._24 - matrix._21;=0A=
	m_planes[3].c =3D matrix._34 - matrix._31;=0A=
	m_planes[3].d =3D matrix._44 - matrix._41;=0A=
	D3DXPlaneNormalize(&amp;m_planes[3], &amp;m_planes[3]);=0A=
=0A=
	// Calculate top plane of frustum.=0A=
	m_planes[4].a =3D matrix._14 - matrix._12; =0A=
	m_planes[4].b =3D matrix._24 - matrix._22;=0A=
	m_planes[4].c =3D matrix._34 - matrix._32;=0A=
	m_planes[4].d =3D matrix._44 - matrix._42;=0A=
	D3DXPlaneNormalize(&amp;m_planes[4], &amp;m_planes[4]);=0A=
=0A=
	// Calculate bottom plane of frustum.=0A=
	m_planes[5].a =3D matrix._14 + matrix._12;=0A=
	m_planes[5].b =3D matrix._24 + matrix._22;=0A=
	m_planes[5].c =3D matrix._34 + matrix._32;=0A=
	m_planes[5].d =3D matrix._44 + matrix._42;=0A=
	D3DXPlaneNormalize(&amp;m_planes[5], &amp;m_planes[5]);=0A=
=0A=
	return;=0A=
}=0A=
</FONT></PRE>
<P align=3D"left"><FONT color=3D"#ffffff" face=3D"Arial" =
size=3D"2">CheckPoint checks if=20
a single point is inside the viewing frustum. This is the most general =
of the=20
four checking algorithms but can be very efficient if used correctly in =
the=20
right situation over the other checking methods. It takes the point and =
checks=20
to see if it is inside all six planes. If the point is inside all six =
then it=20
returns true, otherwise it returns false if not. </FONT></P>
<PRE><FONT color=3D"#aaffaa" face=3D"arial" size=3D"2">bool =
FrustumClass::CheckPoint(float x, float y, float z)=0A=
{=0A=
	int i;=0A=
=0A=
=0A=
	// Check if the point is inside all six planes of the view frustum.=0A=
	for(i=3D0; i&lt;6; i++) =0A=
	{=0A=
		if(D3DXPlaneDotCoord(&amp;m_planes[i], &amp;D3DXVECTOR3(x, y, z)) &lt; =
0.0f)=0A=
		{=0A=
			return false;=0A=
		}=0A=
	}=0A=
=0A=
	return true;=0A=
}=0A=
</FONT></PRE>
<P align=3D"left"><FONT color=3D"#ffffff" face=3D"Arial" =
size=3D"2">CheckCube checks if=20
any of the eight corner points of the cube are inside the viewing =
frustum. It=20
only requires as input the center point of the cube and the radius, it =
uses=20
those to calculate the 8 corner points of the cube. It then checks if =
any one of=20
the corner points are inside all 6 planes of the viewing frustum. If it =
does=20
find a point inside all six planes of the viewing frustum it returns =
true,=20
otherwise it returns false. </FONT></P>
<PRE><FONT color=3D"#aaffaa" face=3D"arial" size=3D"2">bool =
FrustumClass::CheckCube(float xCenter, float yCenter, float zCenter, =
float radius)=0A=
{=0A=
	int i;=0A=
=0A=
=0A=
	// Check if any one point of the cube is in the view frustum.=0A=
	for(i=3D0; i&lt;6; i++) =0A=
	{=0A=
		if(D3DXPlaneDotCoord(&amp;m_planes[i], &amp;D3DXVECTOR3((xCenter - =
radius), (yCenter - radius), (zCenter - radius))) &gt;=3D 0.0f)=0A=
		{=0A=
			continue;=0A=
		}=0A=
		=0A=
		if(D3DXPlaneDotCoord(&amp;m_planes[i], &amp;D3DXVECTOR3((xCenter + =
radius), (yCenter - radius), (zCenter - radius))) &gt;=3D 0.0f)=0A=
		{=0A=
			continue;=0A=
		}=0A=
=0A=
		if(D3DXPlaneDotCoord(&amp;m_planes[i], &amp;D3DXVECTOR3((xCenter - =
radius), (yCenter + radius), (zCenter - radius))) &gt;=3D 0.0f)=0A=
		{=0A=
			continue;=0A=
		}=0A=
=0A=
		if(D3DXPlaneDotCoord(&amp;m_planes[i], &amp;D3DXVECTOR3((xCenter + =
radius), (yCenter + radius), (zCenter - radius))) &gt;=3D 0.0f)=0A=
		{=0A=
			continue;=0A=
		}=0A=
=0A=
		if(D3DXPlaneDotCoord(&amp;m_planes[i], &amp;D3DXVECTOR3((xCenter - =
radius), (yCenter - radius), (zCenter + radius))) &gt;=3D 0.0f)=0A=
		{=0A=
			continue;=0A=
		}=0A=
=0A=
		if(D3DXPlaneDotCoord(&amp;m_planes[i], &amp;D3DXVECTOR3((xCenter + =
radius), (yCenter - radius), (zCenter + radius))) &gt;=3D 0.0f)=0A=
		{=0A=
			continue;=0A=
		}=0A=
=0A=
		if(D3DXPlaneDotCoord(&amp;m_planes[i], &amp;D3DXVECTOR3((xCenter - =
radius), (yCenter + radius), (zCenter + radius))) &gt;=3D 0.0f)=0A=
		{=0A=
			continue;=0A=
		}=0A=
		=0A=
		if(D3DXPlaneDotCoord(&amp;m_planes[i], &amp;D3DXVECTOR3((xCenter + =
radius), (yCenter + radius), (zCenter + radius))) &gt;=3D 0.0f)=0A=
		{=0A=
			continue;=0A=
		}=0A=
=0A=
		return false;=0A=
	}=0A=
=0A=
	return true;=0A=
}=0A=
</FONT></PRE>
<P align=3D"left"><FONT color=3D"#ffffff" face=3D"Arial" =
size=3D"2">CheckSphere checks=20
if the radius of the sphere from the center point is inside all six =
planes of=20
the viewing frustum. If it is outside any of them then the sphere cannot =
be seen=20
and the function will return false. If it is inside all six the function =
returns=20
true that the sphere can be seen. </FONT></P>
<PRE><FONT color=3D"#aaffaa" face=3D"arial" size=3D"2">bool =
FrustumClass::CheckSphere(float xCenter, float yCenter, float zCenter, =
float radius)=0A=
{=0A=
	int i;=0A=
=0A=
=0A=
	// Check if the radius of the sphere is inside the view frustum.=0A=
	for(i=3D0; i&lt;6; i++) =0A=
	{=0A=
		if(D3DXPlaneDotCoord(&amp;m_planes[i], &amp;D3DXVECTOR3(xCenter, =
yCenter, zCenter)) &lt; -radius)=0A=
		{=0A=
			return false;=0A=
		}=0A=
	}=0A=
=0A=
	return true;=0A=
}=0A=
</FONT></PRE>
<P align=3D"left"><FONT color=3D"#ffffff" face=3D"Arial" =
size=3D"2">CheckRectangle works=20
