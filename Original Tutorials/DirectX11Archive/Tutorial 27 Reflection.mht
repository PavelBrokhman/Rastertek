=0A=
bool ReflectionShaderClass::SetShaderParameters(ID3D11DeviceContext* =
deviceContext, D3DXMATRIX worldMatrix, D3DXMATRIX viewMatrix, =0A=
						D3DXMATRIX projectionMatrix, ID3D11ShaderResourceView* texture,=0A=
						ID3D11ShaderResourceView* reflectionTexture, D3DXMATRIX =
reflectionMatrix)=0A=
{=0A=
	HRESULT result;=0A=
	D3D11_MAPPED_SUBRESOURCE mappedResource;=0A=
	MatrixBufferType* dataPtr;=0A=
	unsigned int bufferNumber;=0A=
	ReflectionBufferType* dataPtr2;=0A=
=0A=
=0A=
	// Transpose the matrices to prepare them for the shader.=0A=
	D3DXMatrixTranspose(&amp;worldMatrix, &amp;worldMatrix);=0A=
	D3DXMatrixTranspose(&amp;viewMatrix, &amp;viewMatrix);=0A=
	D3DXMatrixTranspose(&amp;projectionMatrix, &amp;projectionMatrix);=0A=
</FONT></PRE>
<P align=3D"left"><FONT color=3D"#ffffff" face=3D"Arial" =
size=3D"2">Transpose the=20
reflection matrix first as DirectX 11 requires all input matrices to be=20
transposed for memory alignment reasons. </FONT></P>
<PRE><FONT color=3D"#aaffaa" face=3D"arial" size=3D"2">	// Transpose the =
relfection matrix to prepare it for the shader.=0A=
	D3DXMatrixTranspose(&amp;reflectionMatrix, &amp;reflectionMatrix);=0A=
=0A=
	// Lock the matrix constant buffer so it can be written to.=0A=
	result =3D deviceContext-&gt;Map(m_matrixBuffer, 0, =
D3D11_MAP_WRITE_DISCARD, 0, &amp;mappedResource);=0A=
	if(FAILED(result))=0A=
	{=0A=
		return false;=0A=
	}=0A=
=0A=
	// Get a pointer to the data in the matrix constant buffer.=0A=
	dataPtr =3D (MatrixBufferType*)mappedResource.pData;=0A=
=0A=
	// Copy the matrices into the matrix constant buffer.=0A=
	dataPtr-&gt;world =3D worldMatrix;=0A=
	dataPtr-&gt;view =3D viewMatrix;=0A=
	dataPtr-&gt;projection =3D projectionMatrix;=0A=
=0A=
	// Unlock the matrix constant buffer.=0A=
	deviceContext-&gt;Unmap(m_matrixBuffer, 0);=0A=
=0A=
	// Set the position of the matrix constant buffer in the vertex shader.=0A=
	bufferNumber =3D 0;=0A=
=0A=
	// Now set the matrix constant buffer in the vertex shader with the =
updated values.=0A=
	deviceContext-&gt;VSSetConstantBuffers(bufferNumber, 1, =
&amp;m_matrixBuffer);=0A=
</FONT></PRE>
<P align=3D"left"><FONT color=3D"#ffffff" face=3D"Arial" size=3D"2">Lock =
the reflection=20
buffer and copy the reflection matrix into it. After that unlock it and =
set it=20
in the vertex shader. </FONT></P>
<PRE><FONT color=3D"#aaffaa" face=3D"arial" size=3D"2">	// Lock the =
reflection constant buffer so it can be written to.=0A=
	result =3D deviceContext-&gt;Map(m_reflectionBuffer, 0, =
D3D11_MAP_WRITE_DISCARD, 0, &amp;mappedResource);=0A=
	if(FAILED(result))=0A=
	{=0A=
		return false;=0A=
	}=0A=
=0A=
	// Get a pointer to the data in the matrix constant buffer.=0A=
	dataPtr2 =3D (ReflectionBufferType*)mappedResource.pData;=0A=
=0A=
	// Copy the matrix into the reflection constant buffer.=0A=
	dataPtr2-&gt;reflectionMatrix =3D reflectionMatrix;=0A=
=0A=
	// Unlock the reflection constant buffer.=0A=
	deviceContext-&gt;Unmap(m_reflectionBuffer, 0);=0A=
=0A=
	// Set the position of the reflection constant buffer in the vertex =
shader.=0A=
	bufferNumber =3D 1;=0A=
=0A=
	// Now set the reflection constant buffer in the vertex shader with the =
updated values.=0A=
	deviceContext-&gt;VSSetConstantBuffers(bufferNumber, 1, =
&amp;m_reflectionBuffer);=0A=
=0A=
	// Set shader texture resource in the pixel shader.=0A=
	deviceContext-&gt;PSSetShaderResources(0, 1, &amp;texture);=0A=
</FONT></PRE>
<P align=3D"left"><FONT color=3D"#ffffff" face=3D"Arial" size=3D"2">Set =
the reflection=20
texture as the second texture inside the pixel shader. </FONT></P>
<PRE><FONT color=3D"#aaffaa" face=3D"arial" size=3D"2">	// Set the =
reflection texture resource in the pixel shader.=0A=
	deviceContext-&gt;PSSetShaderResources(1, 1, &amp;reflectionTexture);=0A=
=0A=
	return true;=0A=
}=0A=
=0A=
=0A=
void ReflectionShaderClass::RenderShader(ID3D11DeviceContext* =
deviceContext, int indexCount)=0A=
{=0A=
	// Set the vertex input layout.=0A=
	deviceContext-&gt;IASetInputLayout(m_layout);=0A=
=0A=
	// Set the vertex and pixel shaders that will be used to render this =
triangle.=0A=
	deviceContext-&gt;VSSetShader(m_vertexShader, NULL, 0);=0A=
	deviceContext-&gt;PSSetShader(m_pixelShader, NULL, 0);=0A=
=0A=
	// Set the sampler state in the pixel shader.=0A=
	deviceContext-&gt;PSSetSamplers(0, 1, &amp;m_sampleState);=0A=
=0A=
	// Render the triangle.=0A=
	deviceContext-&gt;DrawIndexed(indexCount, 0, 0);=0A=
=0A=
	return;=0A=
}=0A=
</FONT></PRE><BR>
