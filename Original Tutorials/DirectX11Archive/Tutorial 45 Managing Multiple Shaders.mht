	{=0A=
		return false;=0A=
	}=0A=
=0A=
	// Setup the rotation and translation of the second model.=0A=
	m_D3D-&gt;GetWorldMatrix(worldMatrix);=0A=
	D3DXMatrixRotationY(&amp;worldMatrix, rotation);=0A=
	D3DXMatrixTranslation(&amp;translateMatrix, 0.0f, 0.0f, 0.0f);=0A=
	D3DXMatrixMultiply(&amp;worldMatrix, &amp;worldMatrix, =
&amp;translateMatrix); =0A=
</FONT></PRE>
<P align=3D"left"><FONT color=3D"#ffffff" face=3D"Arial" =
size=3D"2">Render the second=20
cube model using the light shader from the ShaderManagerClass object.=20
</FONT></P>
<PRE><FONT color=3D"#aaffaa" face=3D"arial" size=3D"2">	// Render the =
second model using the light shader.=0A=
	m_Model2-&gt;Render(m_D3D-&gt;GetDeviceContext());=0A=
	result =3D =
m_ShaderManager-&gt;RenderLightShader(m_D3D-&gt;GetDeviceContext(), =
m_Model2-&gt;GetIndexCount(), worldMatrix, viewMatrix, projectionMatrix, =0A=
						    m_Model2-&gt;GetTexture(), m_Light-&gt;GetDirection(), =
m_Light-&gt;GetAmbientColor(), m_Light-&gt;GetDiffuseColor(), =0A=
						    m_Camera-&gt;GetPosition(), m_Light-&gt;GetSpecularColor(), =
m_Light-&gt;GetSpecularPower());=0A=
	if(!result)=0A=
	{=0A=
		return false;=0A=
	}=0A=
=0A=
	// Setup the rotation and translation of the third model.=0A=
	m_D3D-&gt;GetWorldMatrix(worldMatrix);=0A=
	D3DXMatrixRotationY(&amp;worldMatrix, rotation);=0A=
	D3DXMatrixTranslation(&amp;translateMatrix, 3.5f, 0.0f, 0.0f);=0A=
	D3DXMatrixMultiply(&amp;worldMatrix, &amp;worldMatrix, =
&amp;translateMatrix); =0A=
</FONT></PRE>
<P align=3D"left"><FONT color=3D"#ffffff" face=3D"Arial" =
size=3D"2">Render the third=20
cube model using the bump map shader from the ShaderManagerClass object. =

</FONT></P>
<PRE><FONT color=3D"#aaffaa" face=3D"arial" size=3D"2">	// Render the =
third model using the bump map shader.=0A=
	m_Model3-&gt;Render(m_D3D-&gt;GetDeviceContext());=0A=
	result =3D =
m_ShaderManager-&gt;RenderBumpMapShader(m_D3D-&gt;GetDeviceContext(), =
m_Model3-&gt;GetIndexCount(), worldMatrix, viewMatrix, projectionMatrix, =0A=
						      m_Model3-&gt;GetColorTexture(), =
m_Model3-&gt;GetNormalMapTexture(), m_Light-&gt;GetDirection(), =0A=
						      m_Light-&gt;GetDiffuseColor());=0A=
	if(!result)=0A=
	{=0A=
		return false;=0A=
	}=0A=
=0A=
	// Present the rendered scene to the screen.=0A=
	m_D3D-&gt;EndScene();=0A=
=0A=
	return true;=0A=
}=0A=
</FONT></PRE><BR>
