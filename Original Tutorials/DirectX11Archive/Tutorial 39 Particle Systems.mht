	}=0A=
=0A=
	return;=0A=
}=0A=
</FONT></PRE>
<P align=3D"left"><FONT color=3D"#ffffff" face=3D"Arial" =
size=3D"2">InitializeBuffers=20
prepares the vertex and index buffer that will be used for rendering the =

particles.  As the particles will be updated every frame the vertex =
buffer will=20
need to be created as a dynamic buffer.  At the beginning there are no =
particles=20
emitted so the vertex buffer will be created empty. </FONT></P>
<PRE><FONT color=3D"#aaffaa" face=3D"arial" size=3D"2">bool =
ParticleSystemClass::InitializeBuffers(ID3D11Device* device)=0A=
{=0A=
	unsigned long* indices;=0A=
	int i;=0A=
	D3D11_BUFFER_DESC vertexBufferDesc, indexBufferDesc;=0A=
	D3D11_SUBRESOURCE_DATA vertexData, indexData;=0A=
	HRESULT result;=0A=
=0A=
=0A=
	// Set the maximum number of vertices in the vertex array.=0A=
	m_vertexCount =3D m_maxParticles * 6;=0A=
=0A=
	// Set the maximum number of indices in the index array.=0A=
	m_indexCount =3D m_vertexCount;=0A=
=0A=
	// Create the vertex array for the particles that will be rendered.=0A=
	m_vertices =3D new VertexType[m_vertexCount];=0A=
	if(!m_vertices)=0A=
	{=0A=
		return false;=0A=
	}=0A=
=0A=
	// Create the index array.=0A=
	indices =3D new unsigned long[m_indexCount];=0A=
	if(!indices)=0A=
	{=0A=
		return false;=0A=
	}=0A=
=0A=
	// Initialize vertex array to zeros at first.=0A=
	memset(m_vertices, 0, (sizeof(VertexType) * m_vertexCount));=0A=
=0A=
	// Initialize the index array.=0A=
	for(i=3D0; i&lt;m_indexCount; i++)=0A=
	{=0A=
		indices[i] =3D i;=0A=
	}=0A=
=0A=
	// Set up the description of the dynamic vertex buffer.=0A=
	vertexBufferDesc.Usage =3D D3D11_USAGE_DYNAMIC;=0A=
	vertexBufferDesc.ByteWidth =3D sizeof(VertexType) * m_vertexCount;=0A=
	vertexBufferDesc.BindFlags =3D D3D11_BIND_VERTEX_BUFFER;=0A=
	vertexBufferDesc.CPUAccessFlags =3D D3D11_CPU_ACCESS_WRITE;=0A=
	vertexBufferDesc.MiscFlags =3D 0;=0A=
	vertexBufferDesc.StructureByteStride =3D 0;=0A=
=0A=
	// Give the subresource structure a pointer to the vertex data.=0A=
	vertexData.pSysMem =3D m_vertices;=0A=
	vertexData.SysMemPitch =3D 0;=0A=
	vertexData.SysMemSlicePitch =3D 0;=0A=
=0A=
	// Now finally create the vertex buffer.=0A=
	result =3D device-&gt;CreateBuffer(&amp;vertexBufferDesc, =
&amp;vertexData, &amp;m_vertexBuffer);=0A=
	if(FAILED(result))=0A=
	{=0A=
		return false;=0A=
	}=0A=
=0A=
	// Set up the description of the static index buffer.=0A=
	indexBufferDesc.Usage =3D D3D11_USAGE_DEFAULT;=0A=
	indexBufferDesc.ByteWidth =3D sizeof(unsigned long) * m_indexCount;=0A=
	indexBufferDesc.BindFlags =3D D3D11_BIND_INDEX_BUFFER;=0A=
	indexBufferDesc.CPUAccessFlags =3D 0;=0A=
	indexBufferDesc.MiscFlags =3D 0;=0A=
	indexBufferDesc.StructureByteStride =3D 0;=0A=
=0A=
	// Give the subresource structure a pointer to the index data.=0A=
	indexData.pSysMem =3D indices;=0A=
	indexData.SysMemPitch =3D 0;=0A=
	indexData.SysMemSlicePitch =3D 0;=0A=
=0A=
	// Create the index buffer.=0A=
	result =3D device-&gt;CreateBuffer(&amp;indexBufferDesc, =
&amp;indexData, &amp;m_indexBuffer);=0A=
	if(FAILED(result))=0A=
	{=0A=
		return false;=0A=
	}=0A=
=0A=
	// Release the index array since it is no longer needed.=0A=
	delete [] indices;=0A=
	indices =3D 0;=0A=
=0A=
	return true;=0A=
}=0A=
</FONT></PRE>
<P align=3D"left"><FONT color=3D"#ffffff" face=3D"Arial" size=3D"2">The =
ShutdownBuffers=20
function releases the vertex and index buffer during shutdown. =
</FONT></P>
<PRE><FONT color=3D"#aaffaa" face=3D"arial" size=3D"2">void =
ParticleSystemClass::ShutdownBuffers()=0A=
{=0A=
	// Release the index buffer.=0A=
	if(m_indexBuffer)=0A=
	{=0A=
		m_indexBuffer-&gt;Release();=0A=
		m_indexBuffer =3D 0;=0A=
	}=0A=
=0A=
	// Release the vertex buffer.=0A=
	if(m_vertexBuffer)=0A=
	{=0A=
		m_vertexBuffer-&gt;Release();=0A=
		m_vertexBuffer =3D 0;=0A=
	}=0A=
=0A=
	return;=0A=
}=0A=
</FONT></PRE>
<P align=3D"left"><FONT color=3D"#ffffff" face=3D"Arial" =
size=3D"2">EmitParticles is=20
called each frame to emit new particles.  It determines when to emit a =
particle=20
based on the frame time and the particles per second variable.  If there =
is a=20
new particle to be emitted then the new particle is created and it =
properties=20
are set.  After that it is inserted into the particle array in Z depth =
order.=20
 The particle array needs to be sorted in correct depth order for =
rendering to=20
work using an alpha blend.  If it is not sorted you will get some visual =

artifacts. </FONT></P>
<PRE><FONT color=3D"#aaffaa" face=3D"arial" size=3D"2">void =
ParticleSystemClass::EmitParticles(float frameTime)=0A=
{=0A=
	bool emitParticle, found;=0A=
	float positionX, positionY, positionZ, velocity, red, green, blue;=0A=
	int index, i, j;=0A=
=0A=
=0A=
	// Increment the frame time.=0A=
	m_accumulatedTime +=3D frameTime;=0A=
=0A=
	// Set emit particle to false for now.=0A=
	emitParticle =3D false;=0A=
	=0A=
	// Check if it is time to emit a new particle or not.=0A=
	if(m_accumulatedTime &gt; (1000.0f / m_particlesPerSecond))=0A=
	{=0A=
		m_accumulatedTime =3D 0.0f;=0A=
		emitParticle =3D true;=0A=
	}=0A=
=0A=
	// If there are particles to emit then emit one per frame.=0A=
	if((emitParticle =3D=3D true) &amp;&amp; (m_currentParticleCount &lt; =
(m_maxParticles - 1)))=0A=
	{=0A=
		m_currentParticleCount++;=0A=
=0A=
		// Now generate the randomized particle properties.=0A=
		positionX =3D (((float)rand()-(float)rand())/RAND_MAX) * =
m_particleDeviationX;=0A=
		positionY =3D (((float)rand()-(float)rand())/RAND_MAX) * =
m_particleDeviationY;=0A=
		positionZ =3D (((float)rand()-(float)rand())/RAND_MAX) * =
m_particleDeviationZ;=0A=
=0A=
		velocity =3D m_particleVelocity + =
(((float)rand()-(float)rand())/RAND_MAX) * m_particleVelocityVariation;=0A=
=0A=
		red   =3D (((float)rand()-(float)rand())/RAND_MAX) + 0.5f;=0A=
		green =3D (((float)rand()-(float)rand())/RAND_MAX) + 0.5f;=0A=
		blue  =3D (((float)rand()-(float)rand())/RAND_MAX) + 0.5f;=0A=
=0A=
		// Now since the particles need to be rendered from back to front for =
blending we have to sort the particle array.=0A=
		// We will sort using Z depth so we need to find where in the list the =
particle should be inserted.=0A=
		index =3D 0;=0A=
		found =3D false;=0A=
		while(!found)=0A=
		{=0A=
			if((m_particleList[index].active =3D=3D false) || =
(m_particleList[index].positionZ &lt; positionZ))=0A=
			{=0A=
				found =3D true;=0A=
			}=0A=
			else=0A=
			{=0A=
				index++;=0A=
			}=0A=
		}=0A=
=0A=
		// Now that we know the location to insert into we need to copy the =
array over by one position from the index to make room for the new =
particle.=0A=
		i =3D m_currentParticleCount;=0A=
		j =3D i - 1;=0A=
=0A=
		while(i !=3D index)=0A=
		{=0A=
			m_particleList[i].positionX =3D m_particleList[j].positionX;=0A=
			m_particleList[i].positionY =3D m_particleList[j].positionY;=0A=
			m_particleList[i].positionZ =3D m_particleList[j].positionZ;=0A=
			m_particleList[i].red       =3D m_particleList[j].red;=0A=
			m_particleList[i].green     =3D m_particleList[j].green;=0A=
			m_particleList[i].blue      =3D m_particleList[j].blue;=0A=
			m_particleList[i].velocity  =3D m_particleList[j].velocity;=0A=
			m_particleList[i].active    =3D m_particleList[j].active;=0A=
			i--;=0A=
			j--;=0A=
		}=0A=
=0A=
		// Now insert it into the particle array in the correct depth order.=0A=
		m_particleList[index].positionX =3D positionX;=0A=
		m_particleList[index].positionY =3D positionY;=0A=
		m_particleList[index].positionZ =3D positionZ;=0A=
		m_particleList[index].red       =3D red;=0A=
		m_particleList[index].green     =3D green;=0A=
		m_particleList[index].blue      =3D blue;=0A=
		m_particleList[index].velocity  =3D velocity;=0A=
		m_particleList[index].active    =3D true;=0A=
	}=0A=
=0A=
	return;=0A=
}=0A=
</FONT></PRE>
<P align=3D"left"><FONT color=3D"#ffffff" face=3D"Arial" size=3D"2">The =
UpdateParticles=20
function is where we update the properties of the particles each frame.  =
In this=20
tutorial we are updating the height position of the particle based on =
its speed=20
which creates the particle water fall effect.  This function can easily =
be=20
extended to do numerous other effects and movement for the particles.=20
</FONT></P>
<PRE><FONT color=3D"#aaffaa" face=3D"arial" size=3D"2">void =
ParticleSystemClass::UpdateParticles(float frameTime)=0A=
{=0A=
	int i;=0A=
=0A=
=0A=
	// Each frame we update all the particles by making them move downwards =
using their position, velocity, and the frame time.=0A=
	for(i=3D0; i&lt;m_currentParticleCount; i++)=0A=
	{=0A=
		m_particleList[i].positionY =3D m_particleList[i].positionY - =
(m_particleList[i].velocity * frameTime * 0.001f);=0A=
	}=0A=
=0A=
	return;=0A=
}=0A=
</FONT></PRE>
<P align=3D"left"><FONT color=3D"#ffffff" face=3D"Arial" size=3D"2">The =
KillParticles=20
function is used to remove particles from the system that have exceeded =
their=20
rendering life time.  This function is called each frame to check if any =

particles should be removed.  In this tutorial the function checks if =
they have=20
dropped below -3.0 height, and if so they are removed and the array is =
shifted=20
back into depth order again. </FONT></P>
<PRE><FONT color=3D"#aaffaa" face=3D"arial" size=3D"2">void =
ParticleSystemClass::KillParticles()=0A=
{=0A=
	int i, j;=0A=
=0A=
=0A=
	// Kill all the particles that have gone below a certain height range.=0A=
	for(i=3D0; i&lt;m_maxParticles; i++)=0A=
	{=0A=
		if((m_particleList[i].active =3D=3D true) &amp;&amp; =
(m_particleList[i].positionY &lt; -3.0f))=0A=
		{=0A=
			m_particleList[i].active =3D false;=0A=
			m_currentParticleCount--;=0A=
=0A=
			// Now shift all the live particles back up the array to erase the =
destroyed particle and keep the array sorted correctly.=0A=
			for(j=3Di; j&lt;m_maxParticles-1; j++)=0A=
			{=0A=
				m_particleList[j].positionX =3D m_particleList[j+1].positionX;=0A=
				m_particleList[j].positionY =3D m_particleList[j+1].positionY;=0A=
				m_particleList[j].positionZ =3D m_particleList[j+1].positionZ;=0A=
				m_particleList[j].red       =3D m_particleList[j+1].red;=0A=
				m_particleList[j].green     =3D m_particleList[j+1].green;=0A=
				m_particleList[j].blue      =3D m_particleList[j+1].blue;=0A=
				m_particleList[j].velocity  =3D m_particleList[j+1].velocity;=0A=
				m_particleList[j].active    =3D m_particleList[j+1].active;=0A=
			}=0A=
		}=0A=
	}=0A=
=0A=
	return;=0A=
}=0A=
</FONT></PRE>
<P align=3D"left"><FONT color=3D"#ffffff" face=3D"Arial" size=3D"2">The =
UpdateBuffers=20
function is called each frame and rebuilds the entire dynamic vertex =
buffer with=20
the updated position of all the particles in the particle system. =
</FONT></P>
<PRE><FONT color=3D"#aaffaa" face=3D"arial" size=3D"2">bool =
ParticleSystemClass::UpdateBuffers(ID3D11DeviceContext* deviceContext)=0A=
{=0A=
	int index, i;=0A=
	HRESULT result;=0A=
	D3D11_MAPPED_SUBRESOURCE mappedResource;=0A=
	VertexType* verticesPtr;=0A=
=0A=
=0A=
	// Initialize vertex array to zeros at first.=0A=
	memset(m_vertices, 0, (sizeof(VertexType) * m_vertexCount));=0A=
=0A=
	// Now build the vertex array from the particle list array.  Each =
particle is a quad made out of two triangles.=0A=
	index =3D 0;=0A=
=0A=
	for(i=3D0; i&lt;m_currentParticleCount; i++)=0A=
	{=0A=
		// Bottom left.=0A=
		m_vertices[index].position =3D D3DXVECTOR3(m_particleList[i].positionX =
- m_particleSize, m_particleList[i].positionY - m_particleSize, =
m_particleList[i].positionZ);=0A=
		m_vertices[index].texture =3D D3DXVECTOR2(0.0f, 1.0f);=0A=
		m_vertices[index].color =3D D3DXVECTOR4(m_particleList[i].red, =
m_particleList[i].green, m_particleList[i].blue, 1.0f);=0A=
		index++;=0A=
=0A=
		// Top left.=0A=
		m_vertices[index].position =3D D3DXVECTOR3(m_particleList[i].positionX =
- m_particleSize, m_particleList[i].positionY + m_particleSize, =
m_particleList[i].positionZ);=0A=
		m_vertices[index].texture =3D D3DXVECTOR2(0.0f, 0.0f);=0A=
		m_vertices[index].color =3D D3DXVECTOR4(m_particleList[i].red, =
m_particleList[i].green, m_particleList[i].blue, 1.0f);=0A=
		index++;=0A=
=0A=
		// Bottom right.=0A=
		m_vertices[index].position =3D D3DXVECTOR3(m_particleList[i].positionX =
+ m_particleSize, m_particleList[i].positionY - m_particleSize, =
m_particleList[i].positionZ);=0A=
		m_vertices[index].texture =3D D3DXVECTOR2(1.0f, 1.0f);=0A=
		m_vertices[index].color =3D D3DXVECTOR4(m_particleList[i].red, =
m_particleList[i].green, m_particleList[i].blue, 1.0f);=0A=
		index++;=0A=
=0A=
		// Bottom right.=0A=
		m_vertices[index].position =3D D3DXVECTOR3(m_particleList[i].positionX =
+ m_particleSize, m_particleList[i].positionY - m_particleSize, =
m_particleList[i].positionZ);=0A=
		m_vertices[index].texture =3D D3DXVECTOR2(1.0f, 1.0f);=0A=
		m_vertices[index].color =3D D3DXVECTOR4(m_particleList[i].red, =
m_particleList[i].green, m_particleList[i].blue, 1.0f);=0A=
		index++;=0A=
=0A=
		// Top left.=0A=
		m_vertices[index].position =3D D3DXVECTOR3(m_particleList[i].positionX =
- m_particleSize, m_particleList[i].positionY + m_particleSize, =
m_particleList[i].positionZ);=0A=
		m_vertices[index].texture =3D D3DXVECTOR2(0.0f, 0.0f);=0A=
		m_vertices[index].color =3D D3DXVECTOR4(m_particleList[i].red, =
m_particleList[i].green, m_particleList[i].blue, 1.0f);=0A=
		index++;=0A=
=0A=
		// Top right.=0A=
		m_vertices[index].position =3D D3DXVECTOR3(m_particleList[i].positionX =
+ m_particleSize, m_particleList[i].positionY + m_particleSize, =
m_particleList[i].positionZ);=0A=
		m_vertices[index].texture =3D D3DXVECTOR2(1.0f, 0.0f);=0A=
		m_vertices[index].color =3D D3DXVECTOR4(m_particleList[i].red, =
m_particleList[i].green, m_particleList[i].blue, 1.0f);=0A=
		index++;=0A=
	}=0A=
	=0A=
	// Lock the vertex buffer.=0A=
	result =3D deviceContext-&gt;Map(m_vertexBuffer, 0, =
D3D11_MAP_WRITE_DISCARD, 0, &amp;mappedResource);=0A=
	if(FAILED(result))=0A=
	{=0A=
		return false;=0A=
	}=0A=
=0A=
	// Get a pointer to the data in the vertex buffer.=0A=
	verticesPtr =3D (VertexType*)mappedResource.pData;=0A=
=0A=
	// Copy the data into the vertex buffer.=0A=
	memcpy(verticesPtr, (void*)m_vertices, (sizeof(VertexType) * =
m_vertexCount));=0A=
=0A=
	// Unlock the vertex buffer.=0A=
	deviceContext-&gt;Unmap(m_vertexBuffer, 0);=0A=
=0A=
	return true;=0A=
}=0A=
</FONT></PRE>
<P align=3D"left"><FONT color=3D"#ffffff" face=3D"Arial" =
size=3D"2">RenderBuffers is=20
used to draw the particle buffers. It places the geometry on the =
pipeline so=20
that the shader can render it. </FONT></P>
<PRE><FONT color=3D"#aaffaa" face=3D"arial" size=3D"2">void =
ParticleSystemClass::RenderBuffers(ID3D11DeviceContext* deviceContext)=0A=
{=0A=
	unsigned int stride;=0A=
	unsigned int offset;=0A=
=0A=
=0A=
	// Set vertex buffer stride and offset.=0A=
	stride =3D sizeof(VertexType); =0A=
	offset =3D 0;=0A=
    =0A=
	// Set the vertex buffer to active in the input assembler so it can be =
rendered.=0A=
	deviceContext-&gt;IASetVertexBuffers(0, 1, &amp;m_vertexBuffer, =
&amp;stride, &amp;offset);=0A=
=0A=
	// Set the index buffer to active in the input assembler so it can be =
rendered.=0A=
	deviceContext-&gt;IASetIndexBuffer(m_indexBuffer, DXGI_FORMAT_R32_UINT, =
0);=0A=
=0A=
	// Set the type of primitive that should be rendered from this vertex =
buffer.=0A=
	=
deviceContext-&gt;IASetPrimitiveTopology(D3D11_PRIMITIVE_TOPOLOGY_TRIANGL=
ELIST);=0A=
=0A=
	return;=0A=
}=0A=
</FONT></PRE><BR>
<P align=3D"left"><FONT color=3D"#ffffff" face=3D"Arial"=20
size=3D"3"><U>Particle.vs</U></FONT></P>
<P align=3D"left"><FONT color=3D"#ffffff" face=3D"Arial" size=3D"2">The =
particle.vs and=20
particle.ps HLSL shader programs are what we use to render the =
particles.  They=20
are the basic texture shader with an added color modifying component.=20
</FONT></P>
<PRE><FONT color=3D"#aaffaa" face=3D"arial" =
size=3D"2">//////////////////////////////////////////////////////////////=
//////////////////=0A=
// Filename: particle.vs=0A=
/////////////////////////////////////////////////////////////////////////=
///////=0A=
=0A=
=0A=
/////////////=0A=
// GLOBALS //=0A=
/////////////=0A=
cbuffer MatrixBuffer=0A=
{=0A=
    matrix worldMatrix;=0A=
    matrix viewMatrix;=0A=
    matrix projectionMatrix;=0A=
};=0A=
=0A=
=0A=
//////////////=0A=
// TYPEDEFS //=0A=
//////////////=0A=
</FONT></PRE>
<P align=3D"left"><FONT color=3D"#ffffff" face=3D"Arial" size=3D"2">The =
two input types=20
both have a color component so that the particle can have an individual =
color=20
that is added to the texture base color. </FONT></P>
<PRE><FONT color=3D"#aaffaa" face=3D"arial" size=3D"2">struct =
VertexInputType=0A=
{=0A=
    float4 position : POSITION;=0A=
    float2 tex : TEXCOORD0;=0A=
    float4 color : COLOR;=0A=
};=0A=
=0A=
struct PixelInputType=0A=
{=0A=
    float4 position : SV_POSITION;=0A=
    float2 tex : TEXCOORD0;=0A=
    float4 color : COLOR;=0A=
};=0A=
=0A=
=0A=
/////////////////////////////////////////////////////////////////////////=
///////=0A=
// Vertex Shader=0A=
/////////////////////////////////////////////////////////////////////////=
///////=0A=
PixelInputType ParticleVertexShader(VertexInputType input)=0A=
{=0A=
    PixelInputType output;=0A=
    =0A=
=0A=
    // Change the position vector to be 4 units for proper matrix =
calculations.=0A=
    input.position.w =3D 1.0f;=0A=
=0A=
    // Calculate the position of the vertex against the world, view, and =
projection matrices.=0A=
    output.position =3D mul(input.position, worldMatrix);=0A=
    output.position =3D mul(output.position, viewMatrix);=0A=
    output.position =3D mul(output.position, projectionMatrix);=0A=
    =0A=
    // Store the texture coordinates for the pixel shader.=0A=
    output.tex =3D input.tex;=0A=
</FONT></PRE>
<P align=3D"left"><FONT color=3D"#ffffff" face=3D"Arial" size=3D"2">The =
color is sent=20
through to the pixel shader here. </FONT></P>
<PRE><FONT color=3D"#aaffaa" face=3D"arial" size=3D"2">    // Store the =
particle color for the pixel shader. =0A=
    output.color =3D input.color;=0A=
=0A=
    return output;=0A=
}=0A=
</FONT></PRE><BR>
<P align=3D"left"><FONT color=3D"#ffffff" face=3D"Arial"=20
size=3D"3"><U>Particle.ps</U></FONT></P>
<PRE><FONT color=3D"#aaffaa" face=3D"arial" =
size=3D"2">//////////////////////////////////////////////////////////////=
//////////////////=0A=
// Filename: particle.ps=0A=
/////////////////////////////////////////////////////////////////////////=
///////=0A=
=0A=
=0A=
/////////////=0A=
// GLOBALS //=0A=
/////////////=0A=
Texture2D shaderTexture;=0A=
SamplerState SampleType;=0A=
=0A=
=0A=
//////////////=0A=
// TYPEDEFS //=0A=
//////////////=0A=
</FONT></PRE>
<P align=3D"left"><FONT color=3D"#ffffff" face=3D"Arial" size=3D"2">The =
PixelInputType=20
has the added color component in the pixel shader also. </FONT></P>
<PRE><FONT color=3D"#aaffaa" face=3D"arial" size=3D"2">struct =
PixelInputType=0A=
{=0A=
    float4 position : SV_POSITION;=0A=
    float2 tex : TEXCOORD0;=0A=
    float4 color : COLOR;=0A=
};=0A=
=0A=
=0A=
/////////////////////////////////////////////////////////////////////////=
///////=0A=
// Pixel Shader=0A=
/////////////////////////////////////////////////////////////////////////=
///////=0A=
float4 ParticlePixelShader(PixelInputType input) : SV_TARGET=0A=
{=0A=
    float4 textureColor;=0A=
    float4 finalColor;=0A=
=0A=
=0A=
    // Sample the pixel color from the texture using the sampler at this =
texture coordinate location.=0A=
    textureColor =3D shaderTexture.Sample(SampleType, input.tex);=0A=
</FONT></PRE>
<P align=3D"left"><FONT color=3D"#ffffff" face=3D"Arial" size=3D"2">Here =
is where we=20
combine the texture color and the input particle color to get the final =
output=20
color. </FONT></P>
<PRE><FONT color=3D"#aaffaa" face=3D"arial" size=3D"2">    // Combine =
the texture color and the particle color to get the final color result.=0A=
    finalColor =3D textureColor * input.color;=0A=
=0A=
    return finalColor;=0A=
}=0A=
</FONT></PRE><BR>
<P align=3D"left"><FONT color=3D"#ffffff" face=3D"Arial"=20
size=3D"3"><U>Particleshaderclass.h</U></FONT></P>
