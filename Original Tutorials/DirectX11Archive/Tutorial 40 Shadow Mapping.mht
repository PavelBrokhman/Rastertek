	result =3D m_DepthShader-&gt;Render(m_D3D-&gt;GetDeviceContext(), =
m_SphereModel-&gt;GetIndexCount(), worldMatrix, lightViewMatrix, =
lightProjectionMatrix);=0A=
	if(!result)=0A=
	{=0A=
		return false;=0A=
	}=0A=
=0A=
	// Reset the world matrix.=0A=
	m_D3D-&gt;GetWorldMatrix(worldMatrix);=0A=
=0A=
	// Setup the translation matrix for the ground model.=0A=
	m_GroundModel-&gt;GetPosition(posX, posY, posZ);=0A=
	D3DXMatrixTranslation(&amp;worldMatrix, posX, posY, posZ);=0A=
=0A=
	// Render the ground model with the depth shader.=0A=
	m_GroundModel-&gt;Render(m_D3D-&gt;GetDeviceContext());=0A=
	result =3D m_DepthShader-&gt;Render(m_D3D-&gt;GetDeviceContext(), =
m_GroundModel-&gt;GetIndexCount(), worldMatrix, lightViewMatrix, =
lightProjectionMatrix);=0A=
	if(!result)=0A=
	{=0A=
		return false;=0A=
	}=0A=
</FONT></PRE>
<P align=3D"left"><FONT color=3D"#ffffff" face=3D"Arial" size=3D"2">Set =
the rendering=20
target back to normal. </FONT></P>
<PRE><FONT color=3D"#aaffaa" face=3D"arial" size=3D"2">	// Reset the =
render target back to the original back buffer and not the render to =
texture anymore.=0A=
	m_D3D-&gt;SetBackBufferRenderTarget();=0A=
=0A=
	// Reset the viewport back to the original.=0A=
	m_D3D-&gt;ResetViewport();=0A=
=0A=
