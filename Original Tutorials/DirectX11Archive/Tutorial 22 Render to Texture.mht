=0A=
	// Release the camera object.=0A=
	if(m_Camera)=0A=
	{=0A=
		delete m_Camera;=0A=
		m_Camera =3D 0;=0A=
	}=0A=
=0A=
	// Release the Direct3D object.=0A=
	if(m_D3D)=0A=
	{=0A=
		m_D3D-&gt;Shutdown();=0A=
		delete m_D3D;=0A=
		m_D3D =3D 0;=0A=
