	void ShutdownShader();=0A=
	void OutputShaderErrorMessage(ID3D10Blob*, HWND, WCHAR*);</FONT><FONT =
color=3D"#aaaaff" face=3D"arial" size=3D"2">=0A=
=0A=
	bool SetShaderParameters(ID3D11DeviceContext*, D3DXMATRIX, D3DXMATRIX, =
D3DXMATRIX, float);</FONT><FONT color=3D"#aaffaa" face=3D"arial" =
size=3D"2">=0A=
	void RenderShader(ID3D11DeviceContext*, int);=0A=
=0A=
private:=0A=
