=0A=
    return output;=0A=
}=0A=
</FONT></PRE><BR>
<P align=3D"left"><FONT color=3D"#ffffff" face=3D"Arial"=20
size=3D"3"><U>Shadow.ps</U></FONT></P>
<PRE><FONT color=3D"#aaffaa" face=3D"arial" =
size=3D"2">//////////////////////////////////////////////////////////////=
//////////////////=0A=
// Filename: shadow.ps=0A=
/////////////////////////////////////////////////////////////////////////=
///////=0A=
=0A=
=0A=
//////////////=0A=
// TEXTURES //=0A=
//////////////=0A=
Texture2D shaderTexture : register(t0);=0A=
Texture2D depthMapTexture : register(t1);=0A=
=0A=
=0A=
///////////////////=0A=
// SAMPLE STATES //=0A=
///////////////////=0A=
SamplerState SampleTypeClamp : register(s0);=0A=
SamplerState SampleTypeWrap  : register(s1);=0A=
=0A=
=0A=
//////////////////////=0A=
// CONSTANT BUFFERS //=0A=
//////////////////////=0A=
</FONT></PRE>
<P align=3D"left"><FONT color=3D"#ffffff" face=3D"Arial" size=3D"2">We =
now use the light=20
direction instead of light position for directional light calculations.=20
</FONT></P>
<PRE><FONT color=3D"#aaffaa" face=3D"arial" size=3D"2">cbuffer =
LightBuffer=0A=
{=0A=
    float4 ambientColor;=0A=
    float4 diffuseColor;</FONT><FONT color=3D"#aaaaff" face=3D"arial" =
size=3D"2">=0A=
    float3 lightDirection;</FONT><FONT color=3D"#aaffaa" face=3D"arial" =
size=3D"2">=0A=
    float padding;=0A=
};=0A=
=0A=
=0A=
//////////////=0A=
// TYPEDEFS //=0A=
//////////////=0A=
struct PixelInputType=0A=
{=0A=
    float4 position : SV_POSITION;=0A=
    float2 tex : TEXCOORD0;=0A=
    float3 normal : NORMAL;=0A=
    float4 lightViewPosition : TEXCOORD1;=0A=
};=0A=
=0A=
=0A=
/////////////////////////////////////////////////////////////////////////=
///////=0A=
// Pixel Shader=0A=
/////////////////////////////////////////////////////////////////////////=
///////=0A=
float4 ShadowPixelShader(PixelInputType input) : SV_TARGET=0A=
{=0A=
    float bias;=0A=
    float4 color;=0A=
    float2 projectTexCoord;=0A=
    float depthValue;=0A=
    float lightDepthValue;=0A=
    float lightIntensity;=0A=
    float4 textureColor;</FONT><FONT color=3D"#aaaaff" face=3D"arial" =
size=3D"2">=0A=
    float3 lightDir;=0A=
</FONT></PRE>
<P align=3D"left"><FONT color=3D"#ffffff" face=3D"Arial" size=3D"2">The =
pixel shader=20
will need to invert the light direction. </FONT></P>
<PRE><FONT color=3D"#aaaaff" face=3D"arial" size=3D"2">    // Invert the =
light direction.=0A=
    lightDir =3D -lightDirection;</FONT><FONT color=3D"#aaffaa" =
face=3D"arial" size=3D"2">=0A=
=0A=
    // Set the bias value for fixing the floating point precision issues.=0A=
    bias =3D 0.001f;=0A=
=0A=
    // Set the default output color to the ambient light value for all =
pixels.=0A=
    color =3D ambientColor;=0A=
=0A=
    // Calculate the projected texture coordinates.=0A=
    projectTexCoord.x =3D  input.lightViewPosition.x / =
input.lightViewPosition.w / 2.0f + 0.5f;=0A=
    projectTexCoord.y =3D -input.lightViewPosition.y / =
input.lightViewPosition.w / 2.0f + 0.5f;=0A=
=0A=
    // Determine if the projected coordinates are in the 0 to 1 range.  =
If so then this pixel is in the view of the light.=0A=
    if((saturate(projectTexCoord.x) =3D=3D projectTexCoord.x) &amp;&amp; =
(saturate(projectTexCoord.y) =3D=3D projectTexCoord.y))=0A=
    {=0A=
        // Sample the shadow map depth value from the depth texture =
using the sampler at the projected texture coordinate location.=0A=
        depthValue =3D depthMapTexture.Sample(SampleTypeClamp, =
projectTexCoord).r;=0A=
=0A=
        // Calculate the depth of the light.=0A=
        lightDepthValue =3D input.lightViewPosition.z / =
input.lightViewPosition.w;=0A=
=0A=
        // Subtract the bias from the lightDepthValue.=0A=
        lightDepthValue =3D lightDepthValue - bias;=0A=
=0A=
        // Compare the depth of the shadow map value and the depth of =
the light to determine whether to shadow or to light this pixel.=0A=
        // If the light is in front of the object then light the pixel, =
if not then shadow this pixel since an object (occluder) is casting a =
shadow on it.=0A=
        if(lightDepthValue &lt; depthValue)=0A=
        {=0A=
</FONT></PRE>
<P align=3D"left"><FONT color=3D"#ffffff" face=3D"Arial" size=3D"2">We =
calculate=20
directional lighting now instead of positional. </FONT></P>
<PRE><FONT color=3D"#aaaaff" face=3D"arial" size=3D"2">            // =
Calculate the amount of light on this pixel.=0A=
            lightIntensity =3D saturate(dot(input.normal, =
lightDir));</FONT><FONT color=3D"#aaffaa" face=3D"arial" size=3D"2">=0A=
=0A=
            if(lightIntensity &gt; 0.0f)=0A=
            {=0A=
                // Determine the final diffuse color based on the =
diffuse color and the amount of light intensity.=0A=
                color +=3D (diffuseColor * lightIntensity);=0A=
=0A=
                // Saturate the final light color.=0A=
                color =3D saturate(color);=0A=
            }=0A=
        }=0A=
    }=0A=
</FONT></PRE>
<P align=3D"left"><FONT color=3D"#ffffff" face=3D"Arial" size=3D"2">I =
have added an=20
additional else clause to handle the case where objects are not inside =
the=20
shadow map area. If we find that a pixel is outside this region then we =
do just=20
regular directional lighting. Note that you can comment out this else =
clause to=20
see exactly where your shadow map range begins and ends. </FONT></P>
<PRE><FONT color=3D"#aaaaff" face=3D"arial" size=3D"2">    else=0A=
    {=0A=
        // If this is outside the area of shadow map range then draw =
things normally with regular lighting.=0A=
        lightIntensity =3D saturate(dot(input.normal, lightDir));=0A=
        if(lightIntensity &gt; 0.0f)=0A=
        {=0A=
            color +=3D (diffuseColor * lightIntensity);=0A=
            color =3D saturate(color);=0A=
        }=0A=
    }</FONT><FONT color=3D"#aaffaa" face=3D"arial" size=3D"2">=0A=
=0A=
    // Sample the pixel color from the texture using the sampler at this =
texture coordinate location.=0A=
    textureColor =3D shaderTexture.Sample(SampleTypeWrap, input.tex);=0A=
=0A=
    // Combine the light and texture color.=0A=
    color =3D color * textureColor;=0A=
=0A=
    return color;=0A=
}=0A=
</FONT></PRE><BR>
