	}=0A=
=0A=
	// Release the depth shader object.=0A=
	if(m_DepthShader)=0A=
	{=0A=
		m_DepthShader-&gt;Shutdown();=0A=
		delete m_DepthShader;=0A=
		m_DepthShader =3D 0;=0A=
	}=0A=
=0A=
	// Release the render to texture object.=0A=
	if(m_RenderTexture)=0A=
	{=0A=
		m_RenderTexture-&gt;Shutdown();=0A=
		delete m_RenderTexture;=0A=
		m_RenderTexture =3D 0;=0A=
	}=0A=
=0A=
	// Release the light object.=0A=
	if(m_Light)=0A=
	{=0A=
		delete m_Light;=0A=
		m_Light =3D 0;=0A=
	}=0A=
=0A=
	// Release the ground model object.=0A=
	if(m_GroundModel)=0A=
	{=0A=
		m_GroundModel-&gt;Shutdown();=0A=
		delete m_GroundModel;=0A=
		m_GroundModel =3D 0;=0A=
	}=0A=
=0A=
	// Release the sphere model object.=0A=
	if(m_SphereModel)=0A=
	{=0A=
		m_SphereModel-&gt;Shutdown();=0A=
		delete m_SphereModel;=0A=
		m_SphereModel =3D 0;=0A=
	}=0A=
=0A=
	// Release the cube model object.=0A=
	if(m_CubeModel)=0A=
	{=0A=
		m_CubeModel-&gt;Shutdown();=0A=
		delete m_CubeModel;=0A=
		m_CubeModel =3D 0;=0A=
	}=0A=
=0A=
	// Release the camera object.=0A=
	if(m_Camera)=0A=
	{=0A=
		delete m_Camera;=0A=
		m_Camera =3D 0;=0A=
	}=0A=
=0A=
	// Release the D3D object.=0A=
	if(m_D3D)=0A=
	{=0A=
		m_D3D-&gt;Shutdown();=0A=
